@using JKApi.Data.DAL;
@{
    var DataModel = ViewBag.ItemList;
    var StageModel = ViewBag.CSstageModel;
    var StageActivity = (List<CSActivity>)ViewBag.StageActivityStaticData;

    CSActivity CSActivity1 = new CSActivity();
    CSActivity CSActivity2 = new CSActivity();
    CSActivity CSActivity3 = new CSActivity();
    CSActivity CSActivity4 = new CSActivity();
    CSActivity CSActivity5 = new CSActivity();


    if (StageActivity != null && StageActivity.Count() > 0)
    {
        CSActivity1 = StageActivity.Where(f => f.StaticDesignItemNo == 1).FirstOrDefault();
        CSActivity2 = StageActivity.Where(f => f.StaticDesignItemNo == 2).FirstOrDefault();
        CSActivity3 = StageActivity.Where(f => f.StaticDesignItemNo == 3).FirstOrDefault();
        CSActivity4 = StageActivity.Where(f => f.StaticDesignItemNo == 4).FirstOrDefault();
        CSActivity5 = StageActivity.Where(f => f.StaticDesignItemNo == 5).FirstOrDefault();

    }
}

