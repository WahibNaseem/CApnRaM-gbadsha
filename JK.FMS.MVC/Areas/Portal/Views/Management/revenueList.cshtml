@model IEnumerable<JKApi.Data.DAL.portal_spGet_R_RevenueList_Result>

@{
    ViewBag.Title = "revenueList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section styles{
    <link href="@Url.Content("~//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("https://cdn.datatables.net/responsive/2.1.1/css/responsive.bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/admin/assets/global/plugins/datatables/plugins/bootstrap/datatables.bootstrap.css")" rel="stylesheet" type="text/css" />

    <style type="text/css">
        .jkcustom-page-title {
            font-size: 18px;
            padding: 3px;
            color: #fff !important;
            background: #3598dc !important;
            text-align: center;
            margin-bottom: 10px;
        }

        .tr-HeaderStyle {
            background-color: #336699;
            color: #fff;
        }
    </style>
}

@using (Html.BeginForm("revenueList", "Management", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    var billMonth = ViewBag.billMonth;
    var billYear = ViewBag.billYear;
    <div class="row">
        <div class="col-md-12">
            <div class="jkcustom-page-title">
                Monthy Revenue for Bill Month/Year @billMonth/@billYear
            </div>
            <div class="portlet box">
                <div class="portlet-title">
                    <div class="row">
                        <input type="button" value="Export to Excel" class="btn fa-pull-left btn-default btn-info" />
                    </div>
                </div>
                <div class="portlet-body">

                    <table class="table table-condensed" id="revenueList">
                        <thead class="thead-inverse">
                            <tr class="tr-HeaderStyle">
                                <th>
                                    @Html.DisplayNameFor(model => model.Region_Name)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Billing_Sub_Total)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Billing_Tax)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Billing_Total)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Supply_Sub_Total)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Supply_Tax)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Supply_Total)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Total_Tax)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Total)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Add_Comm_Billing)
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Region_Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Billing_Sub_Total)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Billing_Tax)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Billing_Total)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Supply_Sub_Total)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Supply_Tax)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Supply_Total)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Total_Tax)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Total)
                                    </td>
                                  <td>@Html.DisplayFor(modelItem => item.Add_Comm_Billing)</td>
                                </tr>
                            }
                        </tbody>

                    </table>

                </div>
            </div>
        </div>
    </div>
}


@section pageplugins{
    <script src="@Url.Content("~/Content/admin/assets/global/scripts/datatable.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/admin/assets/global/plugins/datatables/datatables.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/admin/assets/global/plugins/datatables/plugins/bootstrap/datatables.bootstrap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/admin/assets/global/scripts/app.min.js")" type="text/javascript" )"></script>
    <script src="@Url.Content("~/Content/admin/assets/pages/scripts/table-datatables-managed.min.js")" type="text/javascript"></script>
}

