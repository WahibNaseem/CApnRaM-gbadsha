    function getRevenueByMonthChartData() {
        var monthToAdd = $('#MonthNumber').val();
        var spnStartDate = $("#spnStartDate").text();
        var spnEndDate = $("#spnEndtDate").text();
        var billYear = $('#BillYear').val();
        var billMonth = $('#BillMonth').val();

        $('#RevenueByMonthChart').empty();

        var sURL = '@Url.Action("GetFranchiseRevenueByMonthChartData", "Franchise", new { area = "Portal" })?recordNumber=' + monthToAdd + '&spnStartDate=' + spnStartDate + '&spnEndDate=' + spnEndDate + '&billMonth=' + billMonth + '&billYear=' + billYear;
        $.ajax({
            type: "GET",
            url: sURL,
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            success: function (res) {
                var data = [];
                $.each(res, function (index, value) {
                    var flag = value.RangeName.replace("-", ":");
                    var objData = {
                        //link: "JavaScript:getMonthlyRevenueDetailsByCustomerPopup(" + flag + ");",
                        label: value.RangeName,
                        value: value.TotalRevenue,
                        color: value.ColorCode
                    }
                    data.push(objData);

                });

                FusionCharts.ready(function () {
                    var revenueChart = new FusionCharts({
                        type: 'column3d',
                        renderAt: 'RevenueByMonthChart',
                        width: '100%',
                        height: '350',
                        dataFormat: 'json',
                        dataSource: {
                            "chart": {
                                "caption": "Franchisee Monthly Revenue",
                                "subCaption": "",
                                "xAxisName": "Months/Years",
                                "yAxisName": "Revenues($)",
                                "paletteColors": "#0075c2",
                                "valueFontColor": "#ffffff",
                                "baseFont": "Helvetica Neue,Arial",
                                "captionFontSize": "14",
                                "subcaptionFontSize": "14",
                                "subcaptionFontBold": "0",
                                "placeValuesInside": "1",
                                "labelDisplay": "rotate",
                                "rotateValues": "1",
                                "showShadow": "0",
                                "divlineColor": "#999999",
                                "divLineIsDashed": "1",
                                "divlineThickness": "1",
                                "divLineDashLen": "1",
                                "divLineGapLen": "1",
                                "canvasBgColor": "#ffffff",
                                "theme": "fint"
                            },

                            "data": data
                        }
                    });
                    revenueChart.render();
                });

            },
            error: function () {

            }
        });
    };



