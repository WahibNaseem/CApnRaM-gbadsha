            //$('#modal_BillDetail').modal('show');
            $.blockUI();
            var FranchiseeId = $("#hdnFranchiseeId").val();
            var sURL = '@Url.Action("LeaseBillReportDetail", "Franchise", new { area = "Portal" })?TrNo=' + TxNo.toString();
            console.log(sURL);
