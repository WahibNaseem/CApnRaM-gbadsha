    function scrubvalue(number) {

        var num = ''
        if (number != null) {
            num = number;
            num = num.replace('$', '');
            num = num.replace(',', '');
            num = !isNaN(num) && isFinite(num) && num != '' ? num : 0;

        }
        return num;

    }

    function TransferValidateStepFirst() {
