@section scripts{
    <script>

        function getCurrentUrl() {
            return '@Url.Action("FinderFeeListDataResult", "Franchise", new { area = "Portal" })?regionIds=' + $("#regionlist").val() + '&statusIds=' + ($("#ddlStatus").val() != null ? $("#ddlStatus").val() : '');
        }

        $(window).load(function () {
            if ('@ViewBag.selectedRegionId' == 0) {
                $("#modal_RegionModel").modal({ backdrop: 'static' });
            } else {
                //reloadTable();
                initTable();
            }
        });

        jQuery(document).ready(function () {

            $("#btnRegion").click(function () {
                if ($("#selectregionlist").val() != "") {
                    $("#regionlist option[value='" + $("#selectregionlist").val() + "']").attr("selected", 1);
                    $("#regionlist").multiselect("refresh");
                    //reloadTable();
                    initTable();
                    $(".close").click();
                }
            });

            $("#regionlist").multiselect({
                includeSelectAllOption: true,
                selectAllText: 'Select all',
                buttonWidth: '100%',
                nonSelectedText: 'Select region..',
                dropRight: true,
                numberDisplayed: 2
            });

            $("#regionlist").change(function () {
                //reloadTable();
                initTable();
            });

            //
            $("#btnRegion").click(function () {
                if ($("#selectregionlist").val() != "") {
                    $("#ddlStatus option[value='" + $("#selectregionlist").val() + "']").attr("selected", 1);
                    $("#ddlStatus").multiselect("refresh");
                    //reloadTable();
                    initTable();
                    $(".close").click();
                }
            });

            $("#ddlStatus").multiselect({
                includeSelectAllOption: true,
                selectAllText: 'Select all',
                buttonWidth: '100%',
                nonSelectedText: 'Select Status..',
                dropRight: true,
                numberDisplayed: 2,
                autoclose: true

            });

            $("#ddlStatus").change(function () {
                //reloadTable();
                initTable();
            });

            $("#ddlStatusDetail").multiselect({
                includeSelectAllOption: true,
                selectAllText: 'Select all',
                buttonWidth: '100%',
                nonSelectedText: 'Select Status..',
                dropRight: true,
                numberDisplayed: 2
            });

            $("#ddlStatusDetail").change(function () {
                _FFDetail = [];
                btnOpenDeatilBind();
            });


        });

        $("#btnGenerate").click(function () {
            $("#GeneratedData").show();
        });


        $("#input_Search").keyup(function () {
            $.ajax({
                type: "GET",
                url: '@Url.Action("FranchiseAutoComplete", "Franchise", new { area = "Portal" })',
                data: 'keyword=' + $(this).val(),
                success: function (data) {
                    var typeaheadSource = [];
                    $.each(data, function (key, value) {
                        typeaheadSource.push({ id: value.FranchiseeId, name: value.FranchiseeNo + ' ' + value.Name })
                    });

                    $('#input_Search').typeahead({
                        source: typeaheadSource,
                        updater: function (item) {
                            console.log(item);
                            return item;
                        }
                    });
                }
            });
        });

        function reloadTable() {

            $.blockUI();

            $('#tblfindrfeelist tbody').html('');
            $.ajax({
                type: "GET",
                url: '@Url.Action("FinderFeeListData", "Franchise", new { area = "Portal" })?regionIds=' + $("#regionlist").val() + '&statusIds=' + $("#regionlist").val(),
                success: function (data) {

                    data.forEach(function (item) {
                        trInvoicelist = "";
                        trInvoicelist += '<tr>';
                        trInvoicelist += '<td style="text-align:center;">' + item.RegionName + '</td>';
                        trInvoicelist += '<td style="text-align:center;"><a href="javascript:void(0);" onclick="btnOpenDeatil(' + item.FranchiseeId + ')">' + item.FranchiseeNo + '</a></td>';
                        trInvoicelist += '<td style="text-align:left;">' + item.FranchiseeName + '</td>';
                        trInvoicelist += '<td style="text-align:right;">$' + parseFloat(item.TotalFinderFee, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString() + '</td>';
                        trInvoicelist += '<td style="text-align:right;">$' + parseFloat(item.TotalDownPayment, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString() + '</td>';
                        trInvoicelist += '<td style="text-align:right;">$' + parseFloat(item.Expr1, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString() + '</td>';
                        trInvoicelist += '<td style="text-align:right;">$' + parseFloat(item.PaidAmount, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString() + '</td>';
                        trInvoicelist += '<td style="text-align:right;">$' + parseFloat(item.TotalBalance, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString() + '</td>';
                        trInvoicelist += '<td style="text-align:center;">$' + parseFloat(item.TotalBalance, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString() + '</td>';
                        trInvoicelist += '</tr>';


                        $('#tblfindrfeelist tbody').append(trInvoicelist);
                    });

                    $('#tblfindrfeelist').css('width', '100%');
                    $('#tblfindrfeelist').show();

                    $.unblockUI();

                },
                error: function () {
                    $.unblockUI();
                    alert("Content load failed");
                }
            });
        }
        $(document).on('init.dt', function (e, settings) {
            var api = new $.fn.dataTable.Api(settings);
            if ($(api.table().node()).attr('id') == 'tblfindrfeelist') {
                setPaging();
            }
        });

        function initTable() {

            var ps = $('#pageSize').val();
            var dtable = $('#tblfindrfeelist').dataTable({
                "bAutoWidth": false,
                cache: false,
                "iDisplayLength": ps == '-1' ? 25 : ps,
                "bDestroy": true,
                "aLengthMenu": [[5, 7, 10, 25, 50, -1], [5, 7, 10, 25, 50, "All"]],
                //"dom": "<'headerbox list'<'row'<'col-md-1'l><'col-md-4'f><'#divrightckbox.col-md-5 pull-right text-center'>>><'table-responsive't><'bottom'ip><'clear'>",
                "dom": "<'table-responsive't><'bottom'ip><'clear'>",
                "aaSorting": [[0, "desc"]],
                "sAjaxSource": getCurrentUrl(),
                "sAjaxDataProp": "aadata",  // Internationalisation. For more info refer to http://datatables.net/manual/i18n
                "rowCallback": function (nRow, data) {

                },

                "aoColumns": [
                    {
                        "sName": "region",
                        "bSortable": true,
                        "sClass": "txcenter",
                        "mRender": function (data, type, full) {
                            return '<span>' + full.RegionName + '</span>';
                        }
                    },
                    {
                        "sName": "FranchiseeNo",
                        "bSortable": true,
                        "sClass": "txcenter",
                        "mRender": function (data, type, full) {
                            return '<a href="javascript:void(0);" onclick="btnOpenDeatil(' + full.FranchiseeId + ',' + full.StatusListId + ')">' + full.FranchiseeNo + '</a>';
                            //return "<a onclick='btnOpenDeatil(" + full.FranchiseeId +")'  style='color:#0c9cff' id=" + full.FranchiseeId + "><span style=padding-left:15px>" + full.FranchiseeNo + "</span></a>";
                        }
                    },
                    {
                        "sName": "FranchiseeName",
                        "bSortable": true,
                        "mRender": function (data, type, full) {
                            return '<span>' + full.FranchiseeName + '</span>';
                        }
                    },
                    {
                        "sName": "TotalFinderFee",
                        "bSortable": true,
                        "sClass": "right",
                        "mRender": function (data, type, full) {
                            return '$' + parseFloat(full.TotalFinderFee, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString();
                        }
                    },
                    {
                        "sName": "TotalDownPayment",
                        "bSortable": true,
                        "sClass": "right",
                        "mRender": function (data, type, full) {
