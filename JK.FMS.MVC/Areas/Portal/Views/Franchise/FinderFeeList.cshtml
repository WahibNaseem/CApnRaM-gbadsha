        function setPaging() {
            var z = $("#pageSize").val();
            var table = $("#tblfindrfeelist").DataTable();
            if (z > 0) {
                table.page.len(z).draw();
            }
            else if (z == -1) {
                table.page.len(table.rows().count()).draw();
            }
            else {
                table.page.page.len(25).draw();
