@section scripts{
    <script>
        $(window).load(function () {
            if ('@ViewBag.selectedRegionId' == 0) {
                $("#modal_RegionModel").modal({ backdrop: 'static' });
            }

        });
        var table;
        jQuery(document).ready(function () {

            if ('@ViewBag.ftype' != "") {
                $("#select_timeduration").val('@ViewBag.ftype');
                $("#spnStartDate").html('@ViewBag.fstartDate');
                $("#spnEndtDate").html('@ViewBag.fendDate');
            }




            $(document).on('init.dt', function (e, settings) {
                var api = new $.fn.dataTable.Api(settings);
                if ($(api.table().node()).attr('id') == 'tblFranchiseeFinderFee') {
                    setPaging();
                }
            });

            $('#monthlist').change(function () {
                $('#spnStartDate').html('');
                if ($('#monthlist').val() != '' && $('#yearlist').val() != '')
                    bindFinderReport();
            });

            $('#yearlist').change(function () {
                $('#spnEndtDate').html('');
                if ($('#monthlist').val() != '' && $('#yearlist').val() != '')
                    bindFinderReport();
            });

            $("#input_Search").keyup(function () {

                if ($(this).val() != '') {
                    $("#tblFranchiseeFinderFee tbody>tr").hide()
                    $("#tblFranchiseeFinderFee tbody>tr[relcusn*='" + $(this).val().toUpperCase() + "']").show()
                } else {
                    $("#tblFranchiseeFinderFee tbody>tr").show()
                }


                //var table = $("#CustomerList").DataTable();
                //table.search($(this).val()).draw();
            });

            $("#pageSize").change(function () {
                setPaging();
            });

            //$("#input_Search").keyup(function () {
            //    var table = $("#tblFranchiseeFinderFee").DataTable();
            //    table.search($(this).val()).draw();
            //});
            $("#btnRegion").click(function () {
                if ($("#selectregionlist").val() != "") {
                    $("#regionlist option[value='" + $("#selectregionlist").val() + "']").attr("selected", 1);
                    $("#regionlist").multiselect("refresh");
                    bindFinderReport();
                    $(".close").click();
                }
            });
            $('.modal-dialog').draggable();
            $('#divspandate').show();
            $('#divSD').hide();
            $('#divED').hide();

            firstDay = moment().subtract(0, 'month').startOf('month').toString();
            lastDay = moment().subtract(0, 'month').endOf('month').toString();

            //$('#spnStartDate').html(moment(firstDay).format('L'));
            //$('#spnEndtDate').html(moment(lastDay).format('L'));

            //$('#txtStartDate').inputmask("mm/dd/yyyy", {});
            //$('#txtEndtDate').inputmask("mm/dd/yyyy", {});
            $("#regionlist").multiselect({
                includeSelectAllOption: true,
                selectAllText: 'Select all',
                buttonWidth: '100%',
                nonSelectedText: 'Select region..',
                dropRight: true,
                numberDisplayed: 2,
                onChange: bindFinderReport,
                onSelectAll: bindFinderReport,
                onDeselectAll: bindFinderReport
            });


            $("#btnRegion").click(function () {
                if ($("#selectregionlist").val() != "") {
                    $("#regionlist option[value='" + $("#selectregionlist").val() + "']").attr("selected", 1);
                    $("#regionlist").multiselect("refresh");
                    bindFinderReport();
                    $(".close").click();
                }
            });

            table = $('#tblFranchiseeFinderFee').DataTable({
                "bAutoWidth": false,
                cache: false,
                "iDisplayLength": 25,
                "bDestroy": true,
                "bLengthChange": false,
                //"dom": '<"top">rt<"bottom"lp><"clear">',
                "dom": "<'table-responsive't><'bottom'ip><'clear'>",
            });
            //bindFinderReport();

        });

        function setPaging() {
            var z = $("#pageSize").val();
            var table = $("#tblFranchiseeFinderFee").DataTable();
            if (z > 0) {
                table.page.len(z).draw();
            }
            else if (z == -1) {
                table.page.len(table.rows().count()).draw();
            }
            else {
                table.page.page.len(25).draw();
            }
        }

        var leasereportList;

        function refreshTable() {
            var url = getLeaseListLink();
            leasereportList.api().ajax.url(encodeURI(url)).load();
        }

        function getFinderFeeLink() {
            var _searchtext = $("#input_Search").val();
            var _startDate = $('#spnStartDate').text();
            var _endDate = $('#spnEndtDate').text();
            var r = $('#regionlist').val();
            return '@Url.Action("FinderFeeListDataWithSearch", "Franchise", new { area = "Portal" })?&startDate=' + _startDate + '&endDate=' + _endDate + '&RegionIds=' + (r == null ? '' : r);
        }

        @*function getFinderListLink() {
            var FranchiseeId = $("#hdnFranchiseeId").val();
            return '@Url.Action("GetFinderFeeReport", "Franchise", new { area = "Portal" })?FranchiseeId=' + FranchiseeId + '&FromDate=' + $("#spnStartDate").text() + '&ToDate=' + $("#spnEndDate").text()
        }*@

        function refreshTable() {
            var url = getTableURL();

            $.blockUI();//$.blockUI({ message: $('#throbberBlock'), theme: true, baseZ: 12000 });

            paymentList.DataTable().ajax.url(url).load(function () {
                calculateTotals();
                $.unblockUI();
            });
        }

        function bindFinderReport() {
            var _regionId = $("#regionlist").val();
            var _fromDateVal = $('#spnStartDate').text();
            var _toDateVal = $('#spnEndtDate').text();
            var _monthVal = $('#monthlist').val();
            var _yearVal = $('#yearlist').val();
            var ty = $("#select_timeduration").val();
            if (_monthVal > 0 && _yearVal > 0) {
                var date = new Date(_monthVal + '/01/' + _yearVal);
                var firstDay = new Date(date.getFullYear(), date.getMonth(), 1);
                var lastDay = new Date(date.getFullYear(), date.getMonth() + 1, 0);
                $('#spnStartDate').html(moment(firstDay).format('L'));
                $('#spnEndtDate').html(moment(lastDay).format('L'));

                _fromDateVal = $('#spnStartDate').text();
                _toDateVal = $('#spnEndtDate').text();
            }

            window.location.href = '@Url.Action("MonthlyFinderFeeListReport", "Franchise", new { area = "Portal" })?&startDate=' + _fromDateVal + '&endDate=' + _toDateVal + '&RegionIds=' + (_regionId == null ? '' : _regionId) + '&ftype=' + ty;

        }
        function bindFinderReportOld() {
            table = $('#tblFranchiseeFinderFee').DataTable({
                "bAutoWidth": false,
                cache: false,
                //"order": true,
                "iDisplayLength": 25,
                "bDestroy": true,
                "iDisplayLength": $('#pageSize').val() == -1 ? 25 : $('#pageSize').val(),
                "bLengthChange": false,
