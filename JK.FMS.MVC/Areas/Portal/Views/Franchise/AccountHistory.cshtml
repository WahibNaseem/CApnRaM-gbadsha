}
@section scripts{
    <script>
        jQuery(document).ready(function () {

            $("#input_Search").keyup(function () {
                $.ajax({
                    type: "GET",
                    url: '@Url.Action("FranchiseAutoComplete", "Franchise", new { area = "Portal" })',
                    data: 'keyword=' + $(this).val(),
                    success: function (data) {
                        var typeaheadSource = [];
                        $.each(data, function (key, value) {
                            typeaheadSource.push({ id: value.FranchiseeId, name: value.FranchiseeNo + ' ' + value.Name })
                        });

                        $('#input_Search').typeahead({
                            source: typeaheadSource,
                            updater: function (item) {
                                console.log(item);
                                //$("#hdfCustomerId").val(item.id);
                                //redirect(item.id);
                                return item;
                            }
                        });
                    }
                });
            });


            // Fix header offset
            var fixedHeaderOffset = 0;
            if (App.getViewPort().width < App.getResponsiveBreakpoint('md')) {
                if ($('.page-header').hasClass('page-header-fixed-mobile')) {
                    fixedHeaderOffset = $('.page-header').outerHeight(true);
                }
            } else if ($('body').hasClass('page-header-menu-fixed')) {
                fixedHeaderOffset = $('.page-header-menu').outerHeight(true);
            } else if ($('body').hasClass('page-header-top-fixed')) {
                fixedHeaderOffset = $('.page-header-top').outerHeight(true);
            } else if ($('.page-header').hasClass('navbar-fixed-top')) {
                fixedHeaderOffset = $('.page-header').outerHeight(true);
            } else if ($('body').hasClass('page-header-fixed')) {
                fixedHeaderOffset = 64;
            }

            // Datatable
            var table = $("#tblAccountHistory");

            table.dataTable({
                "language": {
                    "aria": {
                        "sortAscending": ": activate to sort column ascending",
                        "sortDescending": ": activate to sort column descending"
                    },
                    "emptyTable": "No lead data available",
                    "info": "Showing _START_ to _END_ of _TOTAL_ records",
                    "infoEmpty": "No records found",
                    "infoFiltered": "(filtered1 from _MAX_ total records)",
                    "lengthMenu": "Show _MENU_",
                    "search": "Search:",
                    "zeroRecords": "No matching records found",
                    "paginate": {
                        "previous": "Prev",
                        "next": "Next",
                        "last": "Last",
                        "first": "First"
                    }
                },
                "dom": "<'row'<'col-md-8 col-sm-12'i><'col-md-4 col-sm-12'<'table-group-actions pull-right'>>r>t<'row'<'col-md-8 col-sm-12'i><'col-md-4 col-sm-12'>>",
                "bPaginate": false,
                "bInfo": false,
                "bStateSave": false,
                "fnStateSaveParams": function (oSettings, sValue) {
                    $("#lead_table tr.filter .form-control").each(function () {
                        sValue[$(this).attr('name')] = $(this).val();
                    });

                    return sValue;
                },
                colReorder: {
                    fixedColumnsLeft: 1
                },
                fixedHeader: {
                    header: true,
                    headerOffset: fixedHeaderOffset
                },
                "columnDefs": [
                    {
                        'orderable': false,
                        'targets': [0]
                    },
                    {
                        "searchable": false,
                        "targets": [0]
                    },
                    {
                        "className": "dt-left"
                    }
                ],
                buttons: [
                    { extend: 'print' },
                    { extend: 'pdf' },
                    { extend: 'excel' }
                ],
                "ordering": false,

            });
            table.find('.group-checkable').change(function () {
                var set = jQuery(this).attr("data-set");
                var checked = jQuery(this).is(":checked");
                jQuery(set).each(function () {
                    if (checked) {
                        $(this).prop("checked", true);
                        $(this).parents('tr').addClass("active");
                    } else {
                        $(this).prop("checked", false);
                        $(this).parents('tr').removeClass("active");
                    }
                });
            });
            table.on('change', 'tbody tr .checkboxes', function () {
                $(this).parents('tr').toggleClass("active");
            });
            $('.tool-action').on('click', function () {
                var action = $(this).attr('data-action');
                table.DataTable().button(action).trigger();
            });
        })
    </script>
}

