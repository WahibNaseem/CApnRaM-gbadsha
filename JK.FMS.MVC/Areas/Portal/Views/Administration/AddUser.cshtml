<script>




    jQuery(document).ready(function ($) {

        $(".chosen-select").chosen({ no_results_text: "Oops, nothing found!" });

        $('#ddlRegionIds').chosen();
        $('#ddlRoleIds').chosen();


        $('#btnSelectAllRegion').click(function () {
            if ($("#btnSelectAllRegion").is(':checked')) {
                $('#ddlRegionIds option').prop('selected', true);
                $("#ddlRegionIds").trigger("chosen:updated");
            } else {
                $('#ddlRegionIds option').prop('selected', false);
                $("#ddlRegionIds").trigger("chosen:updated");
            }

            var option_all = $("#ddlRegionIds option:selected").map(function () {
                return $(this).val();
            }).get().join(',');

            $("#hdnRegionIds").val(option_all);

        });

        $('#btnSelectAllRole').click(function () {

            if ($("#btnSelectAllRole").is(':checked')) {
                $('#ddlRoleIds option').prop('selected', true);
                $("#ddlRoleIds").trigger("chosen:updated");
            } else {
                $('#ddlRoleIds option').prop('selected', false);
                $("#ddlRoleIds").trigger("chosen:updated");
            }


            var option_all = $("#ddlRoleIds option:selected").map(function () {
                return $(this).val();
            }).get().join(',');

            $("#hdnRoleIds").val(option_all);

        });


        $("#ddlGroupId").change(function () {

            var GorupId = $(this).val();
            $.ajax({
                url: '/Portal/Administration/GetRegionByGroupId',
                type: 'GET',
                data: { GorupId: GorupId },
                dataType: 'json',
                cache: false,
                success: function (response) {
                    $("#hdnRegionIds").val(response.Regions);

                    var str_array = $("#hdnRegionIds").val().split(',');
                    $("#ddlRegionIds").val(str_array).trigger("chosen:updated");
                    //alert($("#ddlTeamUserIds").val());
                }
            });
        });


        $("#ddlRegionIds").change(function () {
            $("#hdnRegionIds").val($(this).val());
            //alert($(this).val());
        });

        $("#ddlRoleIds").change(function () {
            $("#hdnRoleIds").val($(this).val());
            //alert($(this).val());
        });


        $('#txtphone').inputmask("mask", {
            "mask": "(999) 999-9999", removeMaskOnSubmit: true,
        });


        @if (Model != null && Model.RegionIds != null) {
            <text>
        $("#hdnRegionIds").val('@Model.RegionIds');
        $("#ddlRegionIds").val($("#hdnRegionIds").val().split(',')).trigger("chosen:updated");
        $("#ddlRegionIds").trigger("chosen:updated");
        </text>
        }

        @if (Model != null && Model.RoleIds != null) {
                <text>
        $("#hdnRoleIds").val('@Model.RoleIds');
        $("#ddlRoleIds").val($("#hdnRoleIds").val().split(','));
        $("#ddlRoleIds").trigger("chosen:updated");
        </text>
            }

        $('#chksameEmail').change(function () {

            if ($("#chksameEmail").is(':checked')) {
                $('#txtusername').val($('#txtemail').val());
            } else {
                $('#txtusername').val("");
            }

        });

    });

    function ResetPassword(_userid) {
        var Id = _userid;
        if (Id != 0) {
            var sURL = '@Url.Action("UserResetPassword", "Administration", new { area = "Portal" })?UserId=' + Id;
            $.ajax({
                type: "GET",
                url: sURL,
                contentType: "application/json; charset=utf-8",
                datatype: "json",
                success: function (data) {
                    if (data == 1) {
                        swal("Success!", "Email has been sent successfully for Reset Password.", "success");
                    }
                },
                error: function () {
                    alert("Content load failed.");
                }
            });
        }
        else {
            alert('Please select invoice to email.')
        }
    }


</script>

