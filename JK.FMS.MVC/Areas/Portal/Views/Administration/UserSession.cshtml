            var item = $('#UserDetailTracking').dataTable({
                "ordering": false,
                "bAutoWidth": false,
                "iDisplayLength": ps == '-1' ? 25 : ps,
                "bDestroy": true,
                //"aLengthMenu": [[5, 7, 10, 25, 50, -1], [5, 7, 10, 25, 50, "All"]],
