        }
        else
        {
            $("#sd_ArrayLabelValues").val("");
            $("#sd_divarrayItemText").hide();

        }
    });

    function formValidation(PageNoStep)
    {
        var IsValidEntry = true;
        if(PageNoStep=="1") {
            if($("#txtTemplateName").val()=="")
            {
                IsValidEntry = false;
                $("#txtTemplateName").addClass('error');
            }
            else
                $("#txtTemplateName").removeClass('error');

            if($("#ddlFormTemplateTypeList").val()=="")
