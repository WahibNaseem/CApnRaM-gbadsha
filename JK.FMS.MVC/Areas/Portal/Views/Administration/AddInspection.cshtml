    function sd_edit_row(no) {

        

        $("#sd_txtHeaderName").val(document.getElementById("sd_tdHeaderName_" + no).innerHTML);
        $("#sd_txtHeaderOrder").val(document.getElementById("sd_tdHeaderOrder_" + no).innerHTML);
        $("#sd1_ddlFormTypeId").val($("#sd1_FormTypeId_" + no).val());
        $("#sd_txtFormOrder").val(document.getElementById("sd_tdFormOrder_" + no).innerHTML);

        var ObjItem = JSON.parse(document.getElementById("sd_tdLabelName_" + no).innerHTML);
        if(ObjItem!=null)
        {
            $("#sd_txtLabelName").val(ObjItem.Label);

            var selectedText = $("#sd1_ddlFormTypeId option:selected").text().toLowerCase();

            if(selectedText=="checklist" || selectedText=="options" || selectedText=="segment")
            {
                $("#sd_divarrayItemText").show();
                var textValues = "";
                for (var i = 0; i < ObjItem.Labels.length; i++) {
                    textValues +=ObjItem.Labels[i];
                    if(i+1 < ObjItem.Labels.length)
                    {
                        textValues +=",";
                    }
                }
                $("#sd_ArrayLabelValues").val(textValues);
            }
            else if(selectedText=="photocollection")
            {
                $("#sd_divarrayItemText").show();

                $("#lblLabelText").text("Maximum Array Count");

                $("#sd_ArrayLabelValues").val(ObjItem.ItemCount);
            }
        }

        //  $("#sd_txtLabelName").val(document.getElementById("sd_tdLabelName_" + no).innerHTML);
        $("#sd_txtHeaderRowNo").val(parseInt(document.getElementById("sd_tdRowNo_" + no).innerHTML));
    }
    function sd_save_row(no) {

        document.getElementById("sd_tdHeaderName_" + no).innerHTML = $("#sd_txtHeaderName").val();
        document.getElementById("sd_tdHeaderOrder_" + no).innerHTML = $("#sd_txtHeaderOrder").val();
        document.getElementById("sd_tdddlFormTypeId_" + no).innerHTML =$("#sd1_ddlFormTypeId option:selected").text();
        document.getElementById("sd_tdFormOrder_" + no).innerHTML = $("#sd_txtFormOrder").val();
        //document.getElementById("sd_tdLabelName_" + no).innerHTML = $("#sd_txtLabelName").val();

        $("#sd1_FormTypeId_" + no).val($("#sd1_ddlFormTypeId").val());


        var result = $.grep(lstFormType, function(e){ return e.FormTypeId == $("#sd1_ddlFormTypeId").val();});
        if(result!=null && result.length>0)
        {
            if(result[0].MasterValue!="")
            {
                var ObjItem = JSON.parse(result[0].MasterValue);
                if(ObjItem!=null)
                {
                    ObjItem.Label = $("#sd_txtLabelName").val();

                    var selectedText = $("#sd1_ddlFormTypeId option:selected").text().toLowerCase();

                    if(selectedText=="checklist" || selectedText=="options" || selectedText=="segment")
                    {
                        $("#sd_divarrayItemText").hide();
                        var attr = $("#sd_ArrayLabelValues").val().toString().split(',')
                        ObjItem.Labels = [];
                        ObjItem.Values = [];
                        ObjItem.ItemCount = 0;
                        for (var i = 0; i < attr.length; i++) {
                            if(attr[i]!="")
                            {
                                ObjItem.Labels.push(attr[i]);
                                ObjItem.Values.push("false");
                                ObjItem.ItemCount = (parseInt(ObjItem.ItemCount)+1);
                            }
                        }
                    }
                    else if(selectedText=="photocollection")
                    {
                        ObjItem.ItemCount = $("#sd_ArrayLabelValues").val();
                    }

                }
                document.getElementById("sd_tdLabelName_" + no).innerHTML = JSON.stringify(ObjItem);
            }
        }

        sd_TableReset();
    }
