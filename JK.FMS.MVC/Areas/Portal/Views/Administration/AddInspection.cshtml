            }
            else
                $("#ddlAccountTypeList").removeClass('error');

            if($("#ddlServiceTypeList").val()=="")
            {
                IsValidEntry = false;
                $("#ddlServiceTypeList").addClass('error');
            }
            else
                $("#ddlServiceTypeList").removeClass('error');

        }
        else if(PageNoStep=="2") {
            if($("#sd_txtHeaderName").val()=="")
            {
                IsValidEntry = false;
                $("#sd_txtHeaderName").addClass('error');
            }
            else
                $("#sd_txtHeaderName").removeClass('error');

            if($("#sd_txtHeaderOrder").val()=="")
            {
                IsValidEntry = false;
