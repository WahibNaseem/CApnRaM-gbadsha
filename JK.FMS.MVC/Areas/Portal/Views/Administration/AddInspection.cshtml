            if($("#sd_txtLabelName").val()=="")
            {
                IsValidEntry = false;
                $("#sd_txtLabelName").addClass('error');
            }
            else
                $("#sd_txtLabelName").removeClass('error');
        }
        else {
            IsValidEntry = true;
        }
        return IsValidEntry;
