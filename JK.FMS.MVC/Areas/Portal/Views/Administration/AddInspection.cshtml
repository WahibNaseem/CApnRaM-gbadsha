                if(MaxValue < parseInt(targetTable.rows[rowIndex].cells[4].innerText))
                {
                    MaxValue = parseInt(targetTable.rows[rowIndex].cells[4].innerText);
                }

                //targetTable.rows.item(rowIndex).style.display = 'table-row';
            }
        }

        $("#sd1_txtOrder").val(MaxValue+1);

    });

    $("#sd1_ddlFormTypeId").change(function () {

        
        var selectedText = $("#sd1_ddlFormTypeId option:selected").text().toLowerCase();

        if(selectedText=="checklist" || selectedText=="options" || selectedText=="segment" || selectedText=="photocollection")
        {
            $("#sd_divarrayItemText").show();

            if(selectedText=="photocollection")
