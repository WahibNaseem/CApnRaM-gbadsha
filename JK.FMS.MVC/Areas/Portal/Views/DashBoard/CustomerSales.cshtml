<script>
    $(document).ready(function () {
        var scheduleCalendar = $("#calendar");
        scheduleCalendar.fullCalendar('destroy');
        scheduleCalendar.fullCalendar({
            header: {
                left: 'title',
                center: '',
                right: 'prev,next,today,month,agendaWeek'
            },
            defaultView: 'month'
        });

        reCreateCalendar();
        getSalesSchedules();
        getTodaySchedules();
    });

    function reCreateCalendar() {
        var scheduleCalendar = $("#calendar");
        scheduleCalendar.fullCalendar('destroy');
        scheduleCalendar.fullCalendar({
            header: {
                left: 'title',
                center: '',
                right: 'prev,next,today,month,agendaWeek,agendaDay'
            },
            defaultView: 'month'
        });
    }

    function getTodaySchedules() {

        $.ajax({
            url: "/CRM/CustomerSales/GetSchedulesForToday",
            type: "GET",
            dataType: "json",
            cache: false,
            success: function (response) {
                refreshSchedule(response);
            },
            error: function (error) {
                console.log(error);
            }
        });
    }

    var refreshSchedule = function (data) {

        if (data != null) {

            //Empty the Schedule html
            $("#content_schedule").empty();

            $.each(data.schedule, function (key, item) {

                var contentSchedule = '<div class="mt-comment">';
                contentSchedule += '<div class="mt-comment-img">';
                contentSchedule += '<h3 class="mt-date-text text-center">' + ScheduleDate(item.StartDate) + '</h3>';
                contentSchedule += '<h5 class="mt-date-text text-center mt-schedule-monthSpace">' + ScheduleMonth(item.StartDate) + '</h5>';
                contentSchedule += '</div>';
                contentSchedule += '<div class="mt-comment-body">';
                contentSchedule += '<div class="mt-comment-info">';
                contentSchedule += '<span class="mt-title">' + item.Title + '</span>';
                contentSchedule += '</div>';
                contentSchedule += '<div class="mt-comment-info">';
                contentSchedule += '<span class="mt-scheduletime">' + ScheduleTime(item.StartDate) + ' - ' + ScheduleTime(item.EndDate) + '</span>';
                contentSchedule += '</div>';
                contentSchedule += '<div class="mt-Note-text">' + ((item.Description != "" && item.Description != null) ? item.Description : "") + '</div>';
                contentSchedule += '</div>';
                contentSchedule += '</div>';
                contentSchedule += '<h5 style="border-bottom: 1px solid #e7ecf1;"></h5>';
                $("#content_schedule").append(contentSchedule);
            });

        }

        resetAddScheduleForm();
        JK.CRM.CRMApp.hideActivity("#schedule_modalview_footer");


    }

   function getSalesUrl() {
         var types = "2";
         var users = "";
        return '@Url.Action("GetSalesSchedule", "CRMSchedules", new { area = "CRM" })?types=' + types + '&users=' + users;
    }

    function getSalesSchedules() {

        var u = getSalesUrl();

        $.ajax({
            url: u,
            type: "GET",
            dataType: "json",
            cache: false,
            success: function (response) {
                refreshcalendar(response);
            },
            error: function (error) {
                console.log(error);
            }

        });

    }

    function refreshcalendar(data) {
        var scheduleCalendar = $("#calendar");
        scheduleCalendar.fullCalendar('destroy');
        scheduleCalendar.fullCalendar({
            header: {
                left: 'title',
                center: '',
                right: 'prev,next,today,month,agendaWeek,agendaDay'
            },
            eventClick: function (calEvent, jsEvent, view) {

                $("#input_scheduletitle").val(calEvent.title);
                $("#input_schedulelocation").val(calEvent.Location);
                //  calEvent.IsAllDay == true ? $("#checkbox_alldayevent").prop("checked", true) : $("#checkbox_alldayevent").prop("checked", false);
                $("#input_scheduledescription").val(calEvent.Description);

                $("#input_schedulestartdate").val(calEvent.StartDate);
                $("#input_scheduletype").val(calEvent.CRM_ScheduleTypeId);
                $("#input_scheduleenddate").val(calEvent.EndDate);
                $("#input_schedulestarttime").val(calEvent.StartTime);
                $("#input_scheduleendtime").val(calEvent.EndTime);
                $("#newschedule_modalview").modal('show');

            },
            defaultView: 'month',

            events: data.result
        });
    }

    function addNewSchedule() {

        var addnewSchedule = $("#form_addnewschedule");

        $.ajax({
            url: "/CRM/CRMSchedules/AddNewSchedule",
            type: "POST",
            dataType: "json",
            data: $("#form_addnewschedule").serialize(),
            cache: false,
            success: function (response) {
                resetAddScheduleForm();
                getSchedules();
            },
            error: function (error) {
                console.log(error);
            }
        });

    }

    function resetAddScheduleForm() {

        $("#input_scheduletitle").val("");
        $("#input_schedulelocation").val("");
