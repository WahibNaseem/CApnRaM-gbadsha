
@{
    ViewBag.Title = "CallStatisticsReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form class="form-horizontal" role="form" id="form_CustomerReport" action="" method="post">
    <div class="custom-form portlet light">
        <div class="headerbox">
            <div class="row">
                <div class="col-md-12 text-center">
                    Telemarketer Call Statistics<br />
                    For 1/9/2018
                </div>
            </div>
        </div>
        <div class="portlet-body form">
            <div class="form-body">
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover order-column" cellSpacing=1 cellPadding=1 border=0 style="margin-top:0 !important;" id="tblcall">
                                    <thead>
                                        <tr>
                                            <td colSpan=8 align=center>&nbsp;</td>
                                            <td colSpan=2 noWrap align=center>Averages</td>
                                        </tr>
                                        <tr>
                                            <td noWrap align=center>Region</td>
                                            <td width="15%" noWrap align=center>Users</td>
                                            <td width="15%" noWrap align=center>Dials</td>
                                            <td width="15%" noWrap align=center>Contacted</td>
                                            <td width="15%" noWrap align=center>Pct Contacted</td>
                                            <td width="15%" noWrap align=center>Appointments</td>
                                            <td width="15%" noWrap align=center>Pct Cont. to Appts.</td>
                                            <td width="15%" noWrap align=center>Hours</td>
                                            <td width="15%" noWrap align=center>Dials Per Hour</td>
                                            <td width="15%" noWrap align=center>Hours per Appt</td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td  align=center>Buffalo</td>
                                            <td align=center>&nbsp;0&nbsp;</td>
                                            <td align=center>&nbsp;0&nbsp;</td>
                                            <td align=center>&nbsp;0&nbsp;</td>
                                            <td align=center>&nbsp;0.00&nbsp;</td>
                                            <td align=center>&nbsp;0&nbsp;</td>
                                            <td align=center>&nbsp;0.00&nbsp;</td>
                                            <td align=center>&nbsp;0.00</td>
                                            <td align=center>&nbsp;&nbsp;</td>
                                            <td align=center>&nbsp;&nbsp;</td>
                                        </tr>
                                    </tbody>
                                    <tfoot>
                                        <tr style="background: #95a5a6;color: white;">
                                            <td align=center>&nbsp;Totals&nbsp;</td>
                                            <td align=center>&nbsp;0&nbsp;</td>
                                            <td align=center>&nbsp;0&nbsp;</td>
                                            <td align=center>&nbsp;0&nbsp;</td>
                                            <td align=center>&nbsp;0.00&nbsp;</td>
                                            <td align=center>&nbsp;0&nbsp;</td>
                                            <td align=center>&nbsp;0.00&nbsp;</td>
                                            <td align=center>&nbsp;0.00</td>
                                            <td align=center>&nbsp;&nbsp;</td>
                                            <td align=center>&nbsp;&nbsp;</td>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
@section scripts{
    <script>
    jQuery(document).ready(function () {
        $('#tblcall').dataTable({
            "bAutoWidth": false,
            "bSort": false,
            "order": true,
            "iDisplayLength": 25,
            "aLengthMenu": [[5, 7, 10, 25, 50, -1], [5, 7, 10, 25, 50, "All"]],
            "dom": '<"top">rt<"bottom"lp><"clear">',
            "aaSorting": [[0, "desc"]],
        });
    });
    </script>
}