
@{
    ViewBag.Title = "NotQualifiedResult";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form class="form-horizontal" role="form" id="form_CustomerReport" action="" method="post">
    <div class="custom-form portlet light">
        <div class="headerbox">
            <div class="row">
                <div class="col-md-12 text-center">
                    Customer leads changed to Not Qualified status<br />
                    based on calls logged from 12/31/2017 to 1/6/2018
                </div>
            </div>
        </div>
        <div class="portlet-body form">
            <div class="form-body">
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-12">
                           <button type="button" class="btn btn-primary col-md-1" style="margin-right: 4px;">Toggle</button>
                            <button type="button" class="btn btn-primary col-md-1" style="margin-right: 4px;">Select All</button>
                            <button type="button" class="btn btn-primary col-md-1">Relese</button>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-12">
                            * This report ONLY reflects leads whose status was changed to Not Qualified as the result of a logged call.
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover order-column" cellSpacing=1 cellPadding=1 border=0 style="margin-top:0 !important;" id="tblcall">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th style="text-align:center;">Id</th>
                                            <th style="text-align:center;">Company/Address</th>
                                            <th style="text-align:center;">Notes</th>
                                            <th style="text-align:center;">Not Qualified By</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td><input type="checkbox" style="margin-left: 10px;" /></td>
                                            <td style="text-align:center;"><a javascript:void(0);>5315</a></td>
                                            <td style="text-align:center;">Buffalo cardiology & Pulmonary Assoc PC 6460 Main Street</td>
                                            <td style="text-align:center;">weird NIS --> rscoma</td>
                                            <td style="text-align:center;">Rosamond Scoma</td>
                                        </tr>
                                        <tr>
                                            <td><input type="checkbox" style="margin-left: 10px;" /></td>
                                            <td style="text-align:center;"><a javascript:void(0);>2840</a></td>
                                            <td style="text-align:center;">Entertainment Coupons Inc. 13330 Niagara Falls Blvd.</td>
                                            <td style="text-align:center;">NIS now --> rscoma</td>
                                            <td style="text-align:center;">Rosamond Scoma</td>
                                        </tr>
                                        <tr>
                                            <td><input type="checkbox" style="margin-left: 10px;" /></td>
                                            <td style="text-align:center;"><a javascript:void(0);>727</a></td>
                                            <td style="text-align:center;">Packaging Corp. of America 1110 Military Road</td>
                                            <td style="text-align:center;">DISC --> rscoma</td>
                                            <td style="text-align:center;">Rosamond Scoma</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
@section scripts{
    <script>
        jQuery(document).ready(function () {
            $('#tblcall').dataTable({
                "bAutoWidth": false,
                "bSort": false,
                "order": true,
                "iDisplayLength": 25,
                "aLengthMenu": [[5, 7, 10, 25, 50, -1], [5, 7, 10, 25, 50, "All"]],
                "dom": "<'table-responsive't><'bottom'ip><'clear'>",
                "aaSorting": [[0, "desc"]],
            });
        });
    </script>
}

