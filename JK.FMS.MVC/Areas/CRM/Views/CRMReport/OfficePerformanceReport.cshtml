
@{
    ViewBag.Title = "OfficePerformanceReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form class="form-horizontal" role="form" id="form_CustomerReport" action="" method="post">
    <div class="custom-form portlet light">
        <div class="headerbox">
            <div class="row">
                <div class="col-md-12 text-center">
                    Office Performance Results - Signed<br />
                    For 1/13/2018
                </div>
            </div>
        </div>
        <div class="portlet-body form">
            <div class="form-body">
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover order-column" cellSpacing=1 cellPadding=1 border=0 style="margin-top:0 !important;" id="tblcall">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th style="text-align:center;">Region</th>
                                            <th style="text-align:center;">FV Appts</th>
                                            <th style="text-align:center;">Proposal Count</th>
                                            <th style="text-align:center;">Proposal % of FV Appts</th>
                                            <th style="text-align:center;">Total Proposal Amount</th>
                                            <th style="text-align:center;">Avg. Proposal Amount</th>
                                            <th style="text-align:center;">Reported Sales Count</th>
                                            <th style="text-align:center;">Rpt Sales % of Proposals</th>
                                            <th style="text-align:center;">Reported Sales Amount</th>
                                            <th style="text-align:center;">Actual Sales Count</th>
                                            <th style="text-align:center;">Act Sales % of Proposals</th>
                                            <th style="text-align:center;">Actual Sales Amount</th>
                                            <th style="text-align:center;">No. of No Bids</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td align=center><i class="fa fa-hand-o-right" aria-hidden="true"></i></td>
                                            <td style="text-align:center;">Buffalo</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0%</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0%</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0%</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                        </tr>
                                 
                                    </tbody>
                                    <tfoot>
                                        <tr style="background: #95a5a6;color: white;">
                                            <td></td>
                                            <td style="text-align:center;">Totals</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0%</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0%</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0%</td>
                                            <td style="text-align:center;">0</td>
                                            <td style="text-align:center;">0</td>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
@section scripts{
    <script>
        jQuery(document).ready(function () {
            $('#tblcall').dataTable({
                "bAutoWidth": false,
                "bSort": false,
                "order": true,
                "iDisplayLength": 25,
                "aLengthMenu": [[5, 7, 10, 25, 50, -1], [5, 7, 10, 25, 50, "All"]],
                "dom": "<'table-responsive't><'bottom'ip><'clear'>",
                "aaSorting": [[0, "desc"]],
            });
        });
    </script>
}