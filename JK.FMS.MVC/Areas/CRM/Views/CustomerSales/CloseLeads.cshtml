            disabledPotentialDetailField();
            disableContent();

            disableLink();
            $("#contactinfo_edit_button").text("Edit");
            $("#contactinfo_cancel_button").hide();
            $("#potential_edit_button").text("Edit");
            $("#potential_cancel_button").hide();

            // Showing Modal
            $("#calendar_modalview").on('shown.bs.modal', function () {
                $("#calendar").fullCalendar('render');
            });

            $("#calendar_model").on("click", function () {
                $("#calendar_modalview").modal(open).show();
            });

        });



        $('.potentialdetailtab').click(function (e) {

            var tabClickId = $(this).attr("id");


            if (tabClickId == "Potential_tbContactInfo") {
                $('#divLoadStageStatus_contactinfo').html('');
                $('#' + lastDivstagestatus).contents().appendTo('#divLoadStageStatus_contactinfo')
                $('#' + lastDivstagestatus).html('');
                lastDivstagestatus = "divLoadStageStatus_contactinfo";
            } else if (tabClickId == "Potential_tbPotentialDetail") {
