            });

            $('.input_sat').change(function () {
                if (this.checked) {
                    $(".input_sat").attr("Checked", true);
                }
                else {
                    $(".input_sat").attr("Checked", false);
                }
            });

            $('.input_sun').change(function () {
                if (this.checked) {
                    $(".input_sun").attr("Checked", true);
                }
                else {
                    $(".input_sun").attr("Checked", false);
                }
            });

            $(".checkboxcontactinfo_accountworkbook").prop("checked", false);
            $(".checkboxcontactinfo_BidSheetbook").prop("checked", false);
            $(".checkboxcontactinfo_Cleaningbook").prop("checked", false);

            $.fn.dataTableExt.afnFiltering.push(function (oSettings, aData, iDataIndex) {
                return true;
            });


            $(document).on('init.dt', function (e, settings) {
                var api = new $.fn.dataTable.Api(settings);
                if ($(api.table().node()).attr('id') == 'CustomerList') {
                    setPaging();
                }
            });


            /*Call table initialize function*/
            initTable();

            /*Show Potential Table and Potential Div */
            $('#CustomerList').show();
            $('#divcustomerList').show();

            $("#sold_btnDocumentUpload").attr("disabled", "disabled");
            $("#potential_sold_btnDocumentUpload").attr("disabled", "disabled");
            $("#activity_sold_btnDocumentUpload").attr("disabled", "disabled");
            $("#todo_sold_btnDocumentUpload").attr("disabled", "disabled");
            $("#note_sold_btnDocumentUpload").attr("disabled", "disabled");
            $("#document_sold_btnDocumentUpload").attr("disabled", "disabled");
            $("#schedule_sold_btnDocumentUpload").attr("disabled", "disabled");

            $(".checkboxcontactinfo_accountworkbook").attr("disabled", "disabled");
            $(".checkboxcontactinfo_BidSheetbook").attr("disabled", "disabled");
            $(".checkboxcontactinfo_Cleaningbook").attr("disabled", "disabled");

            setupForm();
            disabledfield();
