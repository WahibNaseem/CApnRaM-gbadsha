@section scripts {
    @*<script src="@Url.Content("~/Areas/CRM/Scripts/crm_potentialcustomer.js")"></script>*@

    <script type="text/javascript">

        function getCurrentUrl() { return '@Url.Action("CloseLeadListData", "CustomerSales", new { area = "CRM" })'; }

        function initTable() {

            var ps = $('#pageSize').val();
            var dtable = $('#CustomerList').dataTable({
                "bAutoWidth": false,
                cache: true,
                "iDisplayLength": ps == '-1' ? 10 : ps,
                "bDestroy": true,
                "aLengthMenu": [[5, 7, 10, 25, 50, -1], [5, 7, 10, 25, 50, "All"]],
                //"dom": "<'headerbox list'<'row'<'col-md-1'l><'col-md-4'f><'#divrightckbox.col-md-5 pull-right text-center'>>><'table-responsive't><'bottom'ip><'clear'>",
                "dom": "<'table-responsive't><'bottom'ip><'clear'>",
                "aaSorting": [[0, "asc"]],
                "sAjaxSource": getCurrentUrl(),
                "sAjaxDataProp": "aadata",  // Internationalisation. For more info refer to http://datatables.net/manual/i18n
                "rowCallback": function (nRow, data) {
                },
                "aoColumns": [
                    {
                        "sName": "AccountId",
                        "bSortable": true,
                        "mRender": function (data, type, full) {
                            return '<span>' + full.CRM_AccountId + '</span>';
                        }
                    },
                    {
                        "sName": "CompanyName",
                        "bSortable": true,
                        "sClass": "tabpointer",
                        "mRender": function (data, type, full) {
                            return "<a href='javascript: void(0);' onclick='getLeadDetail(" + full.CRM_AccountId + ")'  style='color:#0c9cff' id=" + full.CRM_AccountId + ">" + full.CompanyName + "</a>";
                        }
                    },
                    {
                        "sName": "FirstName",
                        "bSortable": true,
                        "mRender": function (data, type, full) {
                            return "<span>" + full.Firstname + "</span>";
                        }
                    },
                    {
                        "sName": "Phone",
                        "bSortable": true,
                        "sClass": "alignCenter",
                        "mRender": function (data, type, full) {
                            if (full.PhoneNumber != null) {
                                if (full.PhoneNumber.length == 10)
                                    var value = '(' + full.PhoneNumber.substr(0, 3) + ')' + ' ' + full.PhoneNumber.substr(3, 3) + '-' + full.PhoneNumber.substr(6, 4);
                                return '<span>' + value + '</span>';
                            } else { return ''; }
                        }
                    },
                    {
                        "sName": "AccountTypeName",
                        "bSortable": true,
                        "sClass": "capitaltext",
                        "mRender": function (data, type, full) {
                            return full.AccountTypeName;
                        }
                    },
                    {
                        "sName": "NumberOfLocations",
                        "bSortable": true,
                        "sClass":"alignCenter",
                        "mRender": function (data, type, full) {
                            return full.NumberOfLocations;
                        }
                    },
                    {
                        "sName": "BudgetAmount",
                        "bSortable": true,
                        "sClass": "right contract",
                        "mRender": function (data, type, full) {
                            if (full.BudgetAmount != null) {
                                var value = '$' + full.BudgetAmount.toFixed(0).replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")
                                return '<span>' + value + '</span>';
                            } else { return ''; }
                        }
                    },
                    {
                        "sName": "MonthlyPrice",
                        "bSortable": true,
                        "sClass": "right contract",
                        "mRender": function (data, type, full) {
                            if (full.MonthlyPrice != null) {
                                var value = '$' + full.MonthlyPrice.toFixed(0).replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")
                                return '<span>' + value + '</span>';
                            } else { return ''; }
                        }
                    },
                    {
                        "sName": "ContractAmount",
                        "bSortable": true,
                        "sClass": "right contract",
                        "mRender": function (data, type, full) {
                            if (full.ContractAmount != null) {
                                var value = '$' + full.ContractAmount.toFixed(0).replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1,")
                                return '<span>' + value + '</span>';

                            } else { return ''; }
                        }

                    },
                    {
                        "sName": "StageStatusName",
                        "bSortable": true,
                        "sClass": "alignCenter capitaltext",
                        "mRender": function (data, type, full) {
                            if (full.StageStatusName == "initialcommunication")
                                return '<span id="spnStageStatus_' + full.CRM_AccountId + '" class="label label-sm label-warning" style="background-color:#567ca5;">' + full.StageStatusName + '</span>'
                            if (full.StageStatusName == "fvpresentation")
                                return '<span id="spnStageStatus_' + full.CRM_AccountId + '" class="label label-sm label-warning" style="background-color:#b3b36f;">' + full.StageStatusName + '</span>'
                            if (full.StageStatusName == "bidding")
                                return '<span id="spnStageStatus_' + full.CRM_AccountId + '" class="label label-sm label-warning" style="background-color:#8a7447;">' + full.StageStatusName + '</span>'
                            if (full.StageStatusName == "pdappointment")
                                return '<span id="spnStageStatus_' + full.CRM_AccountId + '" class="label label-sm label-warning">' + full.StageStatusName + '</span>'
                            if (full.StageStatusName == "followup")
                                return '<span id="spnStageStatus_' + full.CRM_AccountId + '" class="label label-sm label-warning" style="background-color:#e67070;">' + full.StageStatusName + '</span>'
                            if (full.StageStatusName == "sold")
                                return '<span id="spnStageStatus_' + full.CRM_AccountId + '" class="label label-sm label-warning" style="background-color:#75923a;">' + full.StageStatusName + '</span>'
                            if (full.StageStatusName == "close")
                                return '<span id="spnStageStatus_' + full.CRM_AccountId + '" class="label label-sm label-warning" style="background-color:#75923a;">' + full.StageStatusName + '</span>'
                        }
                    }
                ]
            });
        }
        /*Search keyp up event*/
        $("#input_Search").keyup(function () {
            var table = $("#CustomerList").DataTable();
            table.search($(this).val()).draw();
        });

        /*page size change event*/
        $("#pageSize").change(function () {
            setPaging();
        })
        //Set Number of Record to show
        function setPaging() {
            var z = $("#pageSize").val();
            var table = $("#CustomerList").DataTable();
            if (z > 0) {
                table.page.len(z).draw();
            }
            else if (z == -1) {
                table.page.len(table.rows().count()).draw();
            }
            else {
                table.page.page.len(10).draw();
            }
        }


        $("#portlettoplist").click(function () {
            $('#divcustomerList').toggle();
        });


        var getLeadDetail = function (id) {
            if (!(isNaN(id))) {
                $("#divBelow").css("display", "block");
                //$("#portlettoplist").click();
                $('#divcustomerList').hide();
                $(".checkboxcontactinfo_accountworkbook").prop("checked", false);
                $(".checkboxcontactinfo_BidSheetbook").prop("checked", false);
                $(".checkboxcontactinfo_Cleaningbook").prop("checked", false);
                /**
               * Make a request   '
               */
                var URL = "/CRM/CustomerSales/GetPotentialDetail?accountId=" + id;

                $.ajax({
                    url: URL,
                    type: "GET",
                    dataType: "json",
                    cache: false,
                    success: function (data) {
                        $.when(BindFVPR(data.result.CRM_AccountCustomerDetailId, data.result.CRM_AccountId, data.result.StageStatus, data))
                            .then(function () {
                                setDetailData(data);
                            });
                    },
                    error: function (error) {
                        JK.CRM.CRMApp.hideActivity("#addlead_modalview_footer");
                        console.log(error.message);
                    }
                });
            }
        }

        $(document).ready(function () {
            $('.input_mon').change(function () {
