<script>
    function UploadDocument() {
        $('#ModelCustomerUploadDocumentPopup').modal('show');
    }

    var lastDivstagestatus = "divLoadStageStatus_contactinfo";
    function BindFVPR(id, accountid, ss, data1) {
        var dfd = $.Deferred();

        var sURL = '@Url.Action("PartialPotentialLoad", "CustomerSales", new { area = "CRM" })?id=' + id + '&accountid=' + accountid + '&ssid=' + ss;
        $.ajax({
            type: "GET",
            url: sURL,
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            success: function (data) {
                $('#' + lastDivstagestatus).html(data);
                setupForm();
                updateContactU(data1);
                dfd.resolve();
            },
            error: function () {
                alert("Content load failed.");
            }
        });

        return dfd.promise();
    }
</script>
