@using JK.Resources


<div class="col-md-7  biddingcontent" id="bidding" style="margin-left:-30px;">
    <div class="row">
        <div class="col-md-12 portlet-align">
            <div class="portlet box div-right portlet-bottom" style="background-color: #a3a9ad;">
                <div class="portlet-title compact">
                    <div class="caption">
                        Bidding Detail
                    </div>
                </div>
                <div class="portlet-body">
                    <form action="#" id="form_bidstagedetail" class="form-horizontal">
                        <div class="form-body">
                            @Html.Hidden("AccountDetailId", "", new { @id = "accountdetailid" })

                            <!--Bidding Documents upload-->
                            @*<div class="form-group" style="display:none;">
                                <label for="select_documentupload" class="control-label col-md-4">Bidding Documents upload</label>

                                <div class="col-md-4">
                                    <form action="" enctype="multipart/form-data">
                                        <div class="fileinput fileinput-new" data-provides="fileinput">
                                            <div class="input-group input-large">
                                                <div class="form-control uneditable-input input-fixed input-medium" data-trigger="fileinput">
                                                    <i class="fa fa-file fileinput-exists"></i>&nbsp;
                                                    <span class="fileinput-filename"></span>
                                                </div>
                                                <span class="input-group-addon btn default btn-file">
                                                    <span class="fileinput-new" id="btnSelect"> Select file </span>
                                                    <span class="contact_removeDiv fileinput-exists"> Change </span>
                                                    <input type="hidden" value="" name="..."><input type="file" id="contact_bidfileDocument" name="files[]" multiple="" />
                                                </span>
                                            </div>
                                        </div>
                                    </form>
                                </div>

                            </div>*@
                            <!--Upload Button-->
                            <div class="form-group">
                                <div class="col-md-8 col-md-offset-1">
                                    <div id="contact_listDiv" class="CRMDocumentDisplay">
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <a class="btn green clruploadedit" id="contact_crm_UploadDocument1" onclick="OpenCRMUploadDocumentPopup()" style="margin-left:-7px;">Upload/Edit</a>
                                </div>
                            </div>

                            <!-- Lead Status -->
                            <div class="form-group">
                                <input type="checkbox" name="service" class="i - checks control-label col-md-1 aawork checkboxcontactinfo_accountworkbook" id="checkboxcontactinfo_accountworkbook" disabled />
                                <div class="col-md-2" style="padding-left:0px;padding-right:0px;">
                                    <label for="select_accountanalysisbook" class="">Account Analysis</label>
                                </div>
                                <input type="checkbox" name="service" class="i - checks control-label col-md-1 aawork checkboxcontactinfo_BidSheetbook" id="checkboxcontactinfo_BidSheetbook" disabled />
                                <div class="col-md-1" style="padding-left: 0px;padding-right: 0px;">
                                    <label for="select_BidSheetbook" class="">Bid Sheet</label>
                                </div>
                                <input type="checkbox" name="service" class="i - checks control-label col-md-1 aawork checkboxcontactinfo_Cleaningbook" id="checkboxcontactinfo_Cleaningbook" disabled />
                                <div class="col-md-2" style="padding-left:0px;padding-right:0px;">
                                    <label for="select_Cleaningbook" class="">Cleaning Schedule</label>
                                </div>
                            </div>

                            <div class="form-group errmsg" style="display:none; text-align:center" id="requiredDocumentsMsg">
                                <span style="color:red;">You must upload the required documents.</span>
                            </div>

                            <h5 class="form-section sbold uppercase"><b>PD Appointment:</b></h5>

                            <!--Meeting Person-->
                            <div class="form-group">
                                <label for="input_meetwith" class="control-label col-md-3">Meeting Person<span class="required">*</span>:</label>
                                <div class="col-md-5" style="padding-left:0px;">
                                    <input id="input_bidding_meetwith" name="name" type="text" data-required="1" class="form-control input-sm clsbiddingconper" placeholder="" />
                                </div>
                                <div class="col-md-2" style="padding-left:0px;width:130px;">
                                    <input type="checkbox" id="chkbiddingmeetingper1" class="clssamebiddingmeetingper" /> Same as Contact
                                </div>
                            </div>

                            <!--Contact Phone-->
                            <div class="form-group">
                                <label for="input_contactphone" class="control-label col-md-3">Contact Phone<span class="required">*</span>:</label>
                                <div class="col-md-5" style="padding-left:0px;">
                                    <input id="input_bidding_contactphone" name="name" type="text" data-required="1" class="form-control input-sm clsbiddingconphone" placeholder="" />
                                </div>
                            </div>

                            <!--Contact Email-->
                            <div class="form-group">
                                <label for="input_contactemail" class="control-label col-md-3">Contact Email<span class="required">*</span>:</label>
                                <div class="col-md-5" style="padding-left:0px;">
                                    <input id="input_bidding_contactemail" name="email" type="text" data-required="1" class="form-control input-sm clsbiddingconemail" placeholder="" />
                                </div>
                            </div>

                            <div class="form-group errmsg" style="display:none; text-align:center" id="dateTooEarlyMsg">
                                <span style="color:red;">Start date must be more recent than previous meetings.</span>
                            </div>

                            <div class="form-group errmsg" style="display:none; text-align:center" id="scheduleConflictMsg">
                                <span style="color:red;">You have a schedule conflict at the selected time.</span>
                            </div>

                            <!--Start Date/Time-->
                            <div class="form-group">

                                <div class="col-md-9">
                                    <!--Start Date/Time-->
                                    <div class="row">
                                        <label for="schedule_start" class="control-label col-md-4">Start Date/Time<span class="required">*</span>:</label>
                                        <div class="col-md-3" style="padding-left:0px;">
                                            <input type="text" class="form-control input-group input-group-sm input-sm date date-picker RemovePeriodvalidation  input_bid_startdate"
                                                   data-date-format="mm/dd/yyyy" id="input_bidding_schedulestartdate" disabled="disabled"/>
                                        </div>
                                        <!--Start Time-->
                                        <div class="col-md-4" style="padding-left:0px;">
                                            <div class="input-icon">
                                                <i class="fa fa-clock-o" style="margin-top:7px;"></i>
                                                <input type="text" class="form-control  input-group input-group-sm input-sm timepicker input_bid_starttime" id="input_bidding_schedulestarttime"  disabled="disabled">
                                            </div>

                                        </div>
                                    </div>

                                    <!--END Date/Time-->
                                    <div class="row">
                                        <label for="schedule_start" class="control-label col-md-4">End Date/Time<span class="required">*</span>:</label>
                                        <div class="col-md-3" style="padding-left:0px;">
                                            <input type="text" class="form-control input-group input-group-sm input-sm date date-picker RemovePeriodvalidation input_bid_enddate" data-date-format="mm/dd/yyyy"
                                                   id="input_bidding_scheduleenddate"  disabled="disabled" />
                                        </div>
                                        <!--End Time-->
                                        <div class="col-md-4" style="padding-left:0px">
                                            <div class="input-icon">
                                                <i class="fa fa-clock-o" style="margin-top:7px;"></i>
                                                <input type="text" class="form-control  input-group input-group-sm input-sm timepicker input_bid_endtime" id="input_bidding_scheduleendtime" value=""  disabled="disabled" />
                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class="col-md-2">
                                    <a class="btn green btn-block" data-toggle="modal" id="calendar_schedules" style="height: 57px; line-height: 57px; padding: 0px; width: 100px">Schedules</a>
                                </div>

                            </div>

                            <div class="form-group">
                                <label for="callback_purpose" class="control-label col-md-3">Purpose<span class="required">*</span>:</label>
                                <div class="col-md-5" style="padding-left:0px;">
                                    @Html.DropDownList("BiddingPurposeTypeList", (SelectList)ViewBag.BiddingPurposeTypeList,"Select", new { @class="form-control input-sm select_bid_purpose", @id = "select_Biddingcontactinfo_Biddingpurpose",@disabled = "disabled" })
                                </div>
                            </div>


                            <!-- Monthly Price -->
                            <div class="form-group">
                                <label for="input_Biddingcontactinfo_monthlyprice" class="control-label col-md-3">Monthly Price<span class="required">*</span>:</label>
                                <div class="col-md-5" style="padding-left:0px;">
                                    <input id="input_Biddingcontactinfo_monthlyprice" onKeyUp="ContactValueKeyPress()" type="text" data-required="1" class="form-control input-sm input_monthlyprice" placeholder="" />
                                </div>
                            </div>

                            <!-- Price Approved -->
                            <div class="form-group">
                                <label for="select_Biddingcontactinfo_priceapproved" class="control-label col-md-3">Price Approved<span class="required">*</span>:</label>
                                <div class="col-md-5" style="padding-left:0px;">
                                    <select id="select_Biddingcontactinfo_priceapproved" class="form-control input-sm select_priceapproved">
                                        <option value="">Select</option>
                                    </select>
                                </div>
                            </div>

                            <!-- Include IC/EW Price -->
                            <div class="form-group">
                                <label for="input_icprice" class="control-label col-md-3">Include IC/EW Price<span class="required">*</span>:</label>
                                <div class="col-md-5" style="padding-left:0px;">
                                    <input id="input_Biddingcontactinfo_includeicprice" type="text" data-required="1" class="form-control input-sm input_includeicprice" placeholder="" />
                                </div>
                            </div>

                            <h5 class="form-section sbold uppercase"><b>@JKCRMResource.note:</b></h5>
                            <!-- Note -->
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.TextArea("note", "", new { @class = "form-control autosizeme textarea_bidnote", rows = "10", style = "font-size: 11px", @id = "text_Biddingcontactinfo_bidgeneralnote"})
                                </div>
                            </div>

                        </div>
                        <div class="form-actions">
                            <div class="row" id="bid_summary_action">@*onclick="validBiddingDetail();"*@
                                <div class="col-md-3 pull-right">
                                    <button  type="button" class="btn green btn-block" id="btnBiddingDetailSubmit" >@CommonResource.btnSave</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
