@using JK.Resources


<!--Franchise Disclosure-->
<div class="col-md-7" id="contact_frandis" style="margin-left:-30px;">
    <div class="row">
        <div class="col-md-12 portlet-align">
            <div class="portlet box div-right portlet-bottom" style="background-color: #a3a9ad;">
                <div class="portlet-title compact">
                    <div class="caption">
                        Franchise Contract Detail
                    </div>
                </div>
                <div class="portlet-body">
                    <form action="#" id="form_fdstagedetail" class="form-horizontal">
                        <div class="form-body">
                            @Html.Hidden("AccountDetailId", "", new { @id = "accountdetailid" })

                            <!--Franchise Contract Documents upload-->
                            <div class="form-group" style="display:none;">
                                <label for="select_documentupload" class="control-label col-md-4">Franchise Disclosure Documents upload</label>
                                <div class="col-md-4">
                                    <form action="" enctype="multipart/form-data">
                                        <div class="fileinput fileinput-new" data-provides="fileinput">
                                            <div class="input-group input-large">
                                                <div class="form-control uneditable-input input-fixed input-medium" data-trigger="fileinput">
                                                    <i class="fa fa-file fileinput-exists"></i>&nbsp;
                                                    <span class="fileinput-filename"></span>
                                                </div>
                                                <span class="input-group-addon btn default btn-file">
                                                    <span class="fileinput-new" id="btnSelect"> Select file </span>
                                                    <span class="contact_removeDiv fileinput-exists"> Change </span>
                                                    <input type="hidden" value="" name="..."><input type="file" id="contact_bidfileDocument" name="files[]" multiple="" />
                                                </span>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>

                            <!--Upload Button-->
                            <div class="form-group">
                                <div class="col-md-9 col-md-offset-1">
                                    <div id="contact_listDiv" class="CRMFranchiseDocumentDisplay">
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <a class="btn green clruploadedit" id="btn_franchisecontractdocupload"  style="margin-left:-7px;">Upload/Edit</a>
                                </div>
                            </div>
                            <!-- Lead Status -->
                            <div class="form-group">
                                <input type="checkbox" name="service" class="i - checks control-label col-md-1 aawork checkbox_questionaire" id="checkbox_questionaire" disabled />
                                <div class="col-md-3" style="padding-left:0px;padding-right:0px;">
                                    <label for="select_accountanalysisbook" class="">Franchise Questionaire</label>
                                </div>
                                <input type="checkbox" name="service" class="i - checks control-label col-md-1 aawork checkbox_acknowledgement" id="checkbox_acknowledgement" disabled />
                                <div class="col-md-3" style="padding-left: 0px;padding-right: 0px;">
                                    <label for="select_BidSheetbook" class="">Signed Acknowledgement of Disclosure(Receipt)</label>
                                </div>
                              
                            </div>
                            <div class="form-group errmsg" style="display:none; text-align:center" id="requiredDocumentsMsg">
                                <span style="color:red;">You must upload the required documents.</span>
                            </div>

                            <!--Prospect Sign Agreement-->
                            <form>
                                <div class="form-group" style="margin-top:5px;">
                                    <label for="radio_signagreement" class="control-label col-md-4" id="lbl_signagreement">Prospect Sign Agreement<span class="required">*</span>:</label>
                                    <div class="col-md-8 fieldspadding-left">
                                        <label class="check-inline" style="margin-top: 6px;">
                                            <input type="radio" name="membership" value="1" class="i-checks" id="radio_yessignagreement" />
                                            <span>Yes</span>
                                        </label>

                                        <label class="check-inline" style="margin-top: 6px; margin-left:40px;">
                                            <input type="radio" name="membership" value="0" class="i-checks" id="radio_nosignagreement" />
                                            <span>No</span>
                                        </label>
                                    </div>
                                </div>
                            </form>


                            <!--Must have Compnay Created -->
                            <div class="form-group">
                                <label for="select_companycreated" class="control-label col-md-4">Company Created<span class="required">*</span>:</label>
                                <div class="col-md-6 fieldspadding-left">
                                    <select id="select_companycreated" class="form-control input-sm" name="options2">
                                        <option value="">Select</option>
                                        <option value="1">LLC</option>
                                        <option value="0">Corporation</option>
                                    </select>
                                </div>
                            </div>

                            <!-- All Principle Disclosed -->
                            <div class="form-group">
                                <input type="checkbox" name="service" class="i -checks control-label col-md-4 col-md-push-2 col-md-sm-4" style="margin-top:12px;" id="check_allprinciple" />
                                <div class="col-md-4">
                                    <label for="all_principleclosed" class="control-label" id="lbl_principleclose">All Principle Closed</label>
                                </div>
                            </div>
                            <!-- Completed Franchise Application -->
                            <form>
                                <div class="form-group" style="margin-top:5px;">
                                    <label for="radio_fdfranchiseapplication" class="control-label col-md-4" id="lbl_completefranchiseapp">Complete Franchise Application<span class="required">*</span>:</label>
                                    <div class="col-md-8 fieldspadding-left">
                                        <label class="check-inline" style="margin-top: 6px;">
                                            <input type="radio" name="membership" value="1" class="i-checks" id="radio_yesfranchiseapp" />
                                            <span>Yes</span>
                                        </label>

                                        <label class="check-inline" style="margin-top: 6px; margin-left:40px;">
                                            <input type="radio" name="membership" value="0" class="i-checks" id="radio_nofranchiseapp" />
                                            <span>No</span>
                                        </label>
                                    </div>
                                </div>
                            </form>

                            <h5 class="form-section sbold uppercase"><b>@JKCRMResource.note:</b></h5>
                            <!-- Note -->
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.TextArea("note", "", new { @class = "form-control autosizeme", rows = "8", style = "font-size: 11px", @id = "note_franchisecontract" })
                                </div>
                            </div>
                        </div>
                        <div class="form-actions">
                            <div class="row" id="franchisecontract_action">
                                <div class="col-md-3 pull-right">
                                    <button id="save_franchisecontract" type="button" class="btn green btn-block" onclick="validateFranchiseContract();">@CommonResource.btnSave</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

