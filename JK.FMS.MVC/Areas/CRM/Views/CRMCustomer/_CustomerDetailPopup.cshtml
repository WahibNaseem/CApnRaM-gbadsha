@model JKViewModels.Customer.FullCustomerViewModel1
<style>
    .AddBackground {
        background-color: #d64e56;
    }
</style>
<div class="modal fade in" id="ModelCustomerDetailsPopup" tabindex="-1" role="basic" aria-hidden="true" style="display: none; padding-right: 17px;">
    <div class="modal-dialog modal-lg" id="RenderCustomerDetailsPopup">

        <div class="modal-content" style="border-radius: 8px;">
            <div class="modal-header headerbox list" style="padding:0px!important;border-bottom: none;">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true" style="margin-top: 12px !important;margin-right: 12px !important;"></button>
                <h4 class="modal-title text-center" style="margin-top: 0px; margin-bottom: 4px;  padding: 7px!important;font-size: 12px;font-weight: bold;background: #3598DC !important;color: #FFF !important;border: 1px solid #3598DC !important;padding: 0px;text-transform: uppercase;">
                    NEW CUSTOMER - @ViewBag.StatusName.ToUpper() UPDATE
                </h4>
            </div>
            <div class="modal-body form form-horizontal" style="padding-top: 1px;margin-top: -5px;">
                <div class="form-body" style="padding: 0px !important;padding-left:15px !important;">
                    <div class="form-group">
                        <div class="row" style="background-color: #4b85cc;color: #fff;padding-top:8px;padding-bottom:8px;margin-left: 10px;margin-right: 25px;margin-top:8px;">
                            <div class="col-md-12">
                                <div class="row">
                                    <div class="col-md-6">
                                        Customer Information : <b> @Html.DisplayFor(one => one.CustomerDetail.CustomerName)  </b>
                                        <input type="hidden" value="@ViewBag.CallFrom" id="hdfCustomerDetailCallFrom" />
                                    </div>
                                    <div class="col-md-6 text-center">Customer # <b>@Model.CustomerDetail.CustomerNo </b></div>
                                    <input type="hidden" id="hdnOfferCustomerid" value="@ViewBag.CustomerID" />
                                </div>
                            </div>
                        </div>
                        <div class="portlet-body" style="padding:10px">
                            <div class="row">
                                <div class="col-md-12" style="padding-left:0px;">
                                    <div class="col-md-8" style="padding-left:0px;border: 1px solid grey; margin-left: -1px;margin-right: 1px;min-height:165px;">
                                        <div class="col-md-12">
                                            <div class="col-md-12">
                                                <div class="form-body" style="padding: 5px;padding-bottom:0px;">
                                                    <div class="form-group mr0">
                                                        <label class="col-md-3 control-label" style="padding-left: 4px;padding-right: 4px;padding-top:0px;"><b>Status:</b></label>
                                                        <div class="col-md-9" style="padding-top:0px;padding-left: 10px;padding-right:4px;">
                                                            <label class="control-label" style="text-align:left!important;padding-top: 0px;">
                                                                @ViewBag.StatusName
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class="form-group mr0">
                                                        <label class="col-md-3 control-label" style="padding-top: 0px;padding-left: 4px;padding-right: 4px;"><b>Account Type:</b></label>
                                                        <div class="col-md-9" style="padding-top: 0px;padding-left: 10px;padding-right: 4px;">
                                                            <label class="control-label" style="text-align:left!important;padding-top: 0px;">@Html.DisplayFor(one => one.CustomerDetail.Account_Type)</label>
                                                        </div>
                                                    </div>
                                                    <div class="form-group mr0">
                                                        <label class="col-md-3 control-label" style="padding-top: 0px;padding-left: 4px;padding-right: 4px;"><b>Contact Name:</b></label>
                                                        <div class="col-md-9" style="padding-top: 0px;padding-left: 10px;padding-right: 4px;">
                                                            <label class="control-label" style="text-align:left!important;padding-top: 0px;">@Html.DisplayFor(one => one.CustomerDetail.ContactName)</label>
                                                        </div>
                                                    </div>
                                                    <div class="form-group mr0">
                                                        <label class="col-md-3 control-label" style="padding-left: 4px;padding-right: 4px;padding-top:0px;"><b>Phone:</b></label>
                                                        <div class="col-md-9" style="padding-left: 10px;padding-right: 4px;padding-top:0px;">
                                                            <label class="control-label" style="text-align:left!important;padding-top: 0px;padding-bottom: 0px;">@(string.IsNullOrEmpty(Model.CustomerDetail.Phone) ? Model.CustomerDetail.CustomerCell : Model.CustomerDetail.Phone)</label>
                                                        </div>
                                                    </div>
                                                    <div class="form-group mr0">
                                                        <label class="col-md-3 control-label" style="padding-left: 4px;padding-right: 4px;padding-top:0px;"><b>Email:</b></label>
                                                        <div class="col-md-9" style="padding-left: 10px;padding-right: 4px;padding-top:0px;">
                                                            <label class="control-label" style="text-align:left!important;padding-top: 0px;">@Html.DisplayFor(one => one.CustomerDetail.Email)</label>
                                                        </div>
                                                    </div>
                                                    <div class="form-group mr0">
                                                        <label class="col-md-3 control-label" style="padding-left: 4px;padding-right: 4px;padding-top:0px;"><b>Address:</b></label>
                                                        <div class="col-md-9" style="padding-left: 10px;padding-right: 4px;padding-top:0px;">
                                                            <label class="control-label" style="text-align:left!important;padding-top: 0px;">
                                                                @Html.DisplayFor(one => one.CustomerDetail.Address) @Html.DisplayFor(one => one.CustomerDetail.Address2)
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="col-md-4" style="padding-right:0px;border: 1px solid grey;min-height:165px;">
                                        <div class="col-md-6">
                                            <div class="form-group mr0" style="margin-top: 13px;">
                                                @if (ViewBag.HasActiveCustomerInfo)
                                                {
                                                    <a class="btn-custom btn btn-primary col-md-11" style="font-size: 11px;" onclick="showEditCustomerInfo(@ViewBag.CustomerID)" id="btnEditCustomerInfo">Edit Customer Info</a>
                                                }
                                                else
                                                {<a class="btn-custom btn btn-primary col-md-11 AddBackground" style="font-size: 11px;" onclick="showEditCustomerInfo(@ViewBag.CustomerID)" id="btnEditCustomerInfo">Edit Customer Info</a>
                                                }
                                            </div>



                                            <div class="form-group mr0">
                                                @*@if (ViewBag.CustomerContract > 0 && (bool)ViewBag.CustomerContractDetail == true)*@
                                                @if (ViewBag.HasActiveContract)
                                                {
                                                    <a href="javascript:void(0);" id="anhContractCA" class="btn-custom btn btn-primary col-md-11" style="font-size: 11px;" onclick="OpenCustomerContractPopup(@ViewBag.CustomerID)">Customer Contract</a>
                                                }
                                                else
                                                {
                                                    <a href="javascript:void(0);" id="anhContractCA" class="btn-custom btn btn-primary col-md-11  ca-nvalid AddBackground" style="font-size: 11px;background-color:#d64e56;" onclick="OpenCustomerContractPopup(@ViewBag.CustomerID)">Customer Contract</a>
                                                }
                                            </div>
                                            <div class="form-group mr0">
                                                @if (ViewBag.HasActiveContractType != 2) { 
                                                    if (ViewBag.HasActiveDistribution)
                                                    {
                                                        @*<a class="btn-custom btn btn-primary col-md-11" style="font-size: 11px;" onclick="showCustomerFranchiseeDistribution(@Model.MaintenanceTempId)">Distribution</a>*@
                                                        <a class="btn-custom btn btn-primary col-md-11" id="anhDistributionCA" style="font-size: 11px;" onclick="BindFranchiseeCustomerDistribution(@Model.CustomerDetail.CustomerId,@ViewBag.DistributionFranchiseeId)">Distribution</a>
                                                    }
                                                    else
                                                    {
                                                        <a class="btn-custom btn btn-primary col-md-11 ca-nvalid" id="anhDistributionCA" style="font-size: 11px;background-color:#d64e56;" onclick="ShowCustomerSuggetionBox()">Distribution</a>
                                                    }
                                                }
                                                <input type="hidden" value="@Model.MaintenanceTempId" id="hdnMaintenanceTempId" />
                                            </div>

                                            <div class="form-group mr0">
                                                @if (ViewBag.HasActiveContractType != 2)
                                                {
                                                    if (ViewBag.Status == 39)
                                                    {
                                                        if (ViewBag.CustomerContract > 0 && ViewBag.TotalDistribution > 0 && (bool)ViewBag.CustomerContractDetail == true)
                                                        {
                                                            if (ViewBag.FindersFeeCustomer > 0)
                                                            {
                                                                <a href="javasscript:void(0)" id="anhFindersFeeCA" class="btn-custom btn btn-primary col-md-11" style="font-size: 11px;" onclick="AddFindersFee(@ViewBag.CustomerID, @ViewBag.DistributionDetailLineNeedsFF,@ViewBag.DistributionId)">Finders Fee</a>

                                                            }
                                                            else
                                                            {
                                                                <a href="javasscript:void(0)" id="anhFindersFeeCA" class="btn-custom btn btn-primary col-md-11 ca-nvalid" style="font-size: 11px;background-color:#d64e56;" onclick="AddFindersFee(@ViewBag.CustomerID, @ViewBag.DistributionDetailLineNeedsFF,@ViewBag.DistributionId)">Finders Fee</a>
                                                            }
                                                        }
                                                        else
                                                        {
                                                            <a href="javasscript:void(0)" id="anhFindersFeeCA" class="btn-custom btn btn-primary col-md-11 ca-nvalid" style="font-size: 11px;background-color:#d64e56;" onclick="FinderFeePopupAlert()">Finders Fee</a>
                                                        }
                                                    }
                                                }
                                                </div>
                                                

                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group mr0" style="margin-top: 13px;">
                                                @if (ViewBag.HasDocumentUploaded == true)
                                                {
                                                    <a href="javascript:void(0)" id="anhUploadDocumentCA" class="btn-custom btn btn-primary col-md-11" style="font-size: 11px;" onclick="OpenCustomerUploadDocumentPopup(@ViewBag.CustomerID)">Documents</a>
                                                }
                                                else
                                                {
                                                    <a href="javascript:void(0)" id="anhUploadDocumentCA" class="btn-custom btn btn-primary col-md-11  ca-nvalid" style="font-size: 11px;background-color:#d64e56;" onclick="OpenCustomerUploadDocumentPopup(@ViewBag.CustomerID)">Documents</a>
                                                }


                                            </div>
                                            @if (ViewBag.Status == 38)
                                            {
                                                <div class="form-group mr0">
                                                    @*<a href="javascript:void(0);" class="btn-custom btn btn-primary col-md-11" style="font-size: 11px;background-color:#d64e56;" onclick="AccountOffringModel(@ViewBag.CustomerID)">Account Offering</a>*@
                                                    <a href="javascript:void(0);" class="btn-custom btn btn-primary col-md-11" style="font-size: 11px;background-color:#d64e56;" onclick="ClickAccountOffring(@ViewBag.CustomerID)">Account Offering</a>
                                                </div>
                                            }
                                            @*<div class="form-group mr0">
                                                <a class="btn-custom btn btn-primary col-md-11" style="font-size:11px;padding-left: 0px;padding-right: 0px;" onclick="">Maintenance</a>
                                            </div>*@
                                            @if (ViewBag.Status == 38)
                                            {
                                                <div class="form-group mr0">
                                                    <a href="javascript:void(0);" class="btn-custom btn btn-primary col-md-11" style="font-size: 11px;background-color:#d64e56;" onclick="NewAccountForm(@ViewBag.CustomerID)">Account Walk-Thru</a>
                                                </div>
                                            }
                                                @if (Model.MaintenanceTempId > 0)
                                                {
                                                    <div class="form-group mr0">
                                                        <a href="javascript:void(0);" class="btn-custom btn btn-primary col-md-11" style="font-size: 11px;background-color:#d64e56;" onclick="OpenFinderFeeForm(@ViewBag.CustomerID)">Account Acceptance</a>
                                                    </div>
                                                }
                                            </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group" style="display:none;">
                                <div class="col-md-12" style="color:red;padding-left:82px;display:none;" id="errmes"><p>Distribution Details is Required.</p> </div>
                                @if (Model.MessagesData != null)
                                {
                                    int count = Model.MessagesData.Count();
                                    var data = Model.MessagesData.LastOrDefault();

                                    foreach (var d in Model.MessagesData)
                                    {
                                        <div class="col-md-12 PendingMessage">
                                            <div class="col-md-2 control-label">
                                                @d.FirstName @d.LastName :<span class="required"> * </span>
                                            </div>
                                            <div class="col-md-6 control-label">
                                                <textarea class="form-control input-sm" style="resize: none;overflow-x:hidden;" readonly="readonly" rows="3">@d.Message</textarea>
                                            </div>
                                            <div class="col-md-2 control-label">
                                            </div>
                                            <div class="col-md-2 control-label">
                                            </div>
                                        </div>
                                    }
                                    if (data != null && data.IsRejacted == true && data.CreatedByChild == Model.USERID)
                                    {
                                        <script>$(".PendingMessage").last().find("textarea").prop("readonly", false)</script>
                                        <div class="col-md-12">
                                            <input type="hidden" id="hdnCustomerId" value="@Model.CustomerDetail.CustomerId" />
                                            <div class="col-md-2 control-label">
                                                @*  Note :<span class="required"> * </span>*@
                                            </div>
                                            <div class="col-md-6 control-label">
                                                @*<textarea class="form-control input-sm" style="resize: none;overflow-x: hidden;" rows="3" id="txtNote"></textarea>*@
                                            </div>
                                            <div class="col-md-2 control-label MainAdmins" style="display:none;">
                                                <input type="button" id="btnReject" onclick="UpdateApproveReject(2)" class="btn btn-primary col-md-11" value="REJECTED" style="margin-bottom:3px;font-size:11px;margin-top:3px;margin-left:-10px !important" />
                                            </div>
                                            <div class="col-md-2 control-label MainAdmins" style="display:none;">
                                                <input type="button" id="btnApprove" onclick="UpdateApproveReject(1)" class="btn btn-primary col-md-11" value="APPROVED" style="font-size:11px;padding-left:0px !important;padding-right:0px !important;margin-top:3px;margin-left:-40px !important" />
                                            </div>
                                            <div style="left: 35px;" class="col-md-2 control-label">
                                                <input type="button" id="btnSubmit" onclick="UpdateRejectMessage(3,$('.PendingMessage').last().find('textarea'))" class="btn btn-primary col-md-11" value="Submit" style="font-size:11px;padding-left:0px !important;padding-right:0px !important;margin-top:3px;margin-left:-40px !important" />
                                            </div>
                                        </div>
                                    }
                                    else
                                    {

                                        <div class="col-md-12">
                                            <input type="hidden" id="hdnCustomerId" value="@Model.CustomerDetail.CustomerId" />
                                            <div class="col-md-2 control-label">
                                                @*Note :<span class="required"> * </span>*@
                                            </div>
                                            <div class="col-md-6 control-label">
                                                @*<textarea class="form-control input-sm" style="resize: none;overflow-x: hidden;" rows="3" id="txtNote"></textarea>*@
                                            </div>
                                            <div class="col-md-2 control-label MainAdmins">
                                                <input type="button" id="btnReject" onclick="UpdateApproveReject(2)" class="btn btn-primary col-md-11" value="REJECTED" style="margin-bottom:3px;font-size:11px;margin-top:3px;margin-left:-10px !important" />
                                            </div>
                                            <div class="col-md-2 control-label MainAdmins">
                                                <input type="button" id="btnApprove" onclick="UpdateApproveReject(1)" class="btn btn-primary col-md-11" value="APPROVED" style="font-size:11px;padding-left:0px !important;padding-right:0px !important;margin-top:3px;margin-left:-40px !important" />
                                            </div>
                                            <div style="display:none;left: 35px;" class="col-md-2 control-label ActionPenidng">
                                                <input type="button" id="btnSubmit" onclick="UpdateApproveReject(4)" class="btn btn-primary col-md-11" value="Submit" style="font-size:11px;padding-left:0px !important;padding-right:0px !important;margin-top:3px;margin-left:-40px !important" />
                                            </div>
                                        </div>
                                    }
                                }
                                else
                                {
                                    <div class="col-md-12">
                                        <input type="hidden" id="hdnCustomerId" value="@Model.CustomerDetail.CustomerId" />
                                        <div class="col-md-2 control-label">
                                            @*Note :<span class="required"> * </span>*@
                                        </div>
                                        <div class="col-md-6 control-label">
                                            @*<textarea class="form-control input-sm" style="resize: none;overflow-x: hidden;" rows="3" id="txtNote"></textarea>*@
                                        </div>
                                        <div class="col-md-2 control-label MainAdmins">
                                            <input type="button" id="btnReject" onclick="UpdateApproveReject(2)" class="btn btn-primary col-md-11" value="REJECTED" style="margin-bottom:3px;font-size:11px;margin-top:3px;margin-left:-10px !important" />
                                        </div>
                                        <div class="col-md-2 control-label MainAdmins">
                                            <input type="button" id="btnApprove" onclick="UpdateApproveReject(1)" class="btn btn-primary col-md-11" value="APPROVED" style="font-size:11px;padding-left:0px !important;padding-right:0px !important;margin-top:3px;margin-left:-40px !important" />
                                        </div>
                                        <div style="display:none;left: 35px;" class="col-md-2 control-label ActionPenidng">
                                            <input type="button" id="btnSubmit" onclick="UpdateApproveReject(4)" class="btn btn-primary col-md-11" value="Submit" style="font-size:11px;padding-left:0px !important;padding-right:0px !important;margin-top:3px;margin-left:-40px !important" />
                                        </div>
                                    </div>
                                }

                            </div>
                            <div class="form-group">
                                <div class="col-md-12">

                                    <div class="col-md-6">
                                        <span style="color:red;display:none;" class="errmsg" id="spnerrmsg">All validation required </span>
                                        @foreach (var item in ViewBag.ValidationItem)
                                        {

                                            <label class="mt-checkbox" style="width:100%;">
                                                <input type="checkbox" id="chkValidationItem_@item.ValidationItemId" class="clsvalitem" value="@item.ValidationItemId">@item.Name<span style="color:red">*</span>
                                                <span></span>
                                            </label>
                                        }
                                    </div>
                                    <div class="col-md-6 control-label">
                                        <span style="color:red;display:none;" class="errmsg" id="pushbackROerrmsg">Note is required for pushing back to RO.</span>
                                        @if (ViewBag.Status == 39) // region accounting
                                        {
                                            <input type="button" class="btn btn-primary" onclick="RegionOperationStatusView()" value="Notes" style="font-size:11px;background-color: #17C4BB;" />
                                        }
                                        <textarea class="form-control input-sm" style="resize: none;overflow-x: hidden;" rows="6" id="txtNote"></textarea>
                                    </div>
                                    <div class="col-md-6 control-label" style="text-align: right">
                                        <input type="button" class="btn btn-primary" onclick="CustomerUpdateStatus(@(ViewBag.Status))" value="Submit" style="min-width:150px;font-size:11px;" />
                                        @if (ViewBag.Status == 39) // region accounting
                                        {
                                            <input type="button" class="btn btn-primary" onclick="CustomerPushBackToRO()" value="Push Back to RO" style="min-width:150px;font-size:11px;" />
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
