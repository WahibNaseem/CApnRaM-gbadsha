    function UpdateApproveReject(status) {

        if($("#hdnMaintenanceTempId").val() == "" || $("#hdnMaintenanceTempId").val() == 0 || $("#hdnMaintenanceTempId").val() == undefined)
        {
            $("#errmes").show();
            return false;
        }

        if ($('#txtNote').val().trim() == '') {
            $('#txtNote').css('border-color', 'red');
            return false;
        } else { $('#txtNote').css('border-color', ''); }

        var Id = $("#hdnCustomerId").val();
        if (Id > 0) {

            $.ajax({
                url: '@Url.Action("UpdateApproveReject", "Customer")',
                dataType: "json",
                data: { CustomerId: Id, Note: $("#txtNote").val(), Status: status },
                traditional: true,
                contentType: "application/json; charset=utf-8",
                success: function () {
                    window.location.href = window.location.href;
                },
                error: function () { }
            });
        }
    }

    function UpdateRejectMessage(status,ctl) {

        if($("#hdnMaintenanceTempId").val() == "" || $("#hdnMaintenanceTempId").val() == 0 || $("#hdnMaintenanceTempId").val() == undefined)
        {
            $("#errmes").show();
            return false;
        }

        if ($(ctl).val().trim() == '') {
            $(ctl).css('border-color', 'red');
            return false;
        } else { $(ctl).css('border-color', ''); }

        var Id = $("#hdnCustomerId").val();
        if (Id > 0) {

            $.ajax({
                url: '@Url.Action("UpdateApproveReject", "Customer")',
                dataType: "json",
                data: { CustomerId: Id, Note: $(ctl).val(), Status: status },
                traditional: true,
                contentType: "application/json; charset=utf-8",
                success: function () {
                    window.location.href = window.location.href;
                },
                error: function () { }
            });
        }
    }

    function CustomerUpdateStatus(status) {
        $('.errmsg').hide();
        var isval = true;
        var errStr = '';

        $(".clsvalitem").each(function () {
            if (!$(this).is(":checked")) {
                if (isval == true) {
                    isval = false;
                }
            }
        })


        if ($('#anhContractCA[class*=ca-nvalid]').length > 0) {
            errStr = (errStr != '' ? errStr + ', Contract and Contract Detail' : 'Contract and Contract Detail');
            isval = false;
        }

        if ($('#anhDistributionCA[class*=ca-nvalid]').length > 0 ) {
            errStr = (errStr != '' ? errStr + ', Distribution' : 'Distribution');
            isval = false;
        }

        if ($('#anhFindersFeeCA[class*=ca-nvalid]').length > 0) {
            errStr = (errStr != '' ? errStr + ', FindersFee' : 'FindersFee');
            isval = false;
        }
        if (!isval) {
            errStr = (errStr != '' ? errStr + ' and All validation required' : 'All validation required');
        }


        if (!isval) {
            swal({ title: errStr, type:"warning" })
            return;
        }

        var Id = $("#hdnCustomerId").val();

        if (Id > 0) {
            var strIds=$('.clsvalitem:checked').map(function(){
                return this.value;
            }).get().join(',');

            $.ajax({
                url: '@Url.Action("CustomerUpdateStatus", "CRMCustomer")',
                dataType: "json",
                data: { CustomerId: Id, Note: $("#txtNote").val(), Status: status,valIds:strIds},
                traditional: true,
                contentType: "application/json; charset=utf-8",
                success: function () {
                    window.location.href = window.location.href;
                },
                error: function () { }
            });
        }

    }

