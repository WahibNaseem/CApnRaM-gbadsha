                    </div>
                    <div class="row" style="padding-top:8px;padding-bottom:8px;margin-left: 0px;margin-right: 0px;min-height: 100px;">
                        <div class="col-md-12">
                            @foreach (var item in ViewBag.ValidationItemOld)
                            {
                                if (((List<JKApi.Data.DAL.Validation>)ViewBag.ValidationItemResult).Any(x => x.ValidationItemId == item.ValidationItemId))
                                {
                                    <label class="mt-checkbox" style="width:100%;">
                                        <input type="checkbox" id="chkValidationItem_@item.ValidationItemId" value="@item.ValidationItemId" checked disabled>@item.Name
                                        <span></span>
                                    </label>
                                }
                                else
                                {
                                    @*<label class="mt-checkbox" style="width:100%;">
                                            <input type="checkbox" id="chkValidationItem_@item.ValidationItemId" value="@item.ValidationItemId" checked>@item.Name
                                            <span></span>
                                        </label>*@
                                }
                            }
                        </div>
                        <div class="col-md-12" style="min-height: 100px;margin-top: 20px;">
                            <div class="col-md-2 right" style="text-align:right">
                                <strong>Notes:</strong>
                            </div>
                            <div class="col-md-8">
                                @if (ViewBag.statusNote != null)
                                {
                                    <textarea readonly class="form-control input-sm" style="background-color: white;resize: none;overflow-x: hidden;" rows="3" id="txtNote">@ViewBag.statusNote</textarea>
                                    @*<strong>@ViewBag.statusNote</strong>*@

                                }
                                else
                                {
                                    <textarea readonly class="form-control input-sm" style="background-color: white;resize: none;overflow-x: hidden;" rows="3" id="txtNote"></textarea>
                                }

                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade custom-model custom-form-design" id="NewAccountDataModel" tabindex="-1" role="basic" aria-hidden="true" style="display: none;">
