                                </div>
                                <label for="select_billingfrequency" class="control-label col-md-3">Billing Frequency:</label>
                                <div class="col-md-3">
                                    @Html.DropDownList("FrequencyListModel", (SelectList)ViewBag.FrequencyListModel, null, new { @class = "form-control input-sm", required = "required", @id = "soldmodal_FrequencyListModel", @disabled = "disabled" })
                                </div>
                            </div>

                            <!--Clean Time AND Clean Frequency-->
                            <div class="form-group" style="margin-top:5px;">
                                <label for="select_cleantime" class="control-label col-md-3">Clean Time:</label>
                                <div class="col-md-3">
                                    <input type="text" class="form-control input-sm" id="soldmodal_cleantime" disabled="disabled" />
                                </div>
                                <label for="select_cleanfrequency" class="control-label col-md-3">Clean Frequency:</label>
                                <div class="col-md-3">
                                    @Html.DropDownList("ContractDetail_CleanFrequency", (SelectList)ViewBag.CleanFrequencyListModel, null, new { @class = "form-control input-sm", required = "required", @id = "soldmodal_ContractDetail_CleanFrequency", @disabled = "disabled" })
                                </div>
                            </div>
                        </form>

                        <!--Cleaning Day-->
                        <form action="#" id="form_sold_cleaningday" class="form-horizontal">
                            <div class="form-group" style="margin-top:4px;" id="weekdays">
                                <label for="select_cleanday" class="control-label col-md-3" style="padding-top: 12px;">Cleaning Day:</label>
                                <div class="col-md-9" id="sold">
                                    <div class="mt-checkbox-inline">
                                        <label class="mt-checkbox">
                                            <input type="checkbox" id="soldmodal_mon" value="1" disabled="disabled"> Mon
                                            <span></span>
                                        </label>
                                        <label class="mt-checkbox">
                                            <input type="checkbox" id="soldmodal_tue" value="2" disabled="disabled"> Tue
                                            <span></span>
                                        </label>
                                        <label class="mt-checkbox">
                                            <input type="checkbox" id="soldmodal_wed" value="3" disabled="disabled"> Wed
                                            <span></span>
                                        </label>
                                        <label class="mt-checkbox">
                                            <input type="checkbox" id="soldmodal_thu" value="4" disabled="disabled"> Thu
                                            <span></span>
                                        </label>
                                        <label class="mt-checkbox">
                                            <input type="checkbox" id="soldmodal_fri" value="5" disabled="disabled"> Fri
                                            <span></span>
                                        </label>
                                        <label class="mt-checkbox">
                                            <input type="checkbox" id="soldmodal_sat" value="6" disabled="disabled"> Sat
                                            <span></span>
                                        </label>
                                        <label class="mt-checkbox">
                                            <input type="checkbox" id="soldmodal_sun" value="7" disabled="disabled"> Sun
                                            <span></span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <!-- Note -->
                        <form action="#" class="form-horizontal">
                            <div class="form-group">
                                <label for="select_stagestatus" class="control-label col-md-4">Note<span class="required"></span>:</label>
                                <div class="col-md-6">
                                    @Html.TextArea("note", "", new { @class = "form-control autosizeme", rows = "6", style = "font-size: 11px", @id = "text_soldmodal", disabled = "disabled" })
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
</div>
